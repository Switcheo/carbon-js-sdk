{
  "output_id": "mkr_usd",
  "templates": [
    {
      "template_id": "full_response_handler",
      "adapters": [
        {
          "adapter": "unresponsive_handler",
          "config": {
            "strategy": "use_last",
            "grace_duration": "120"
          }
        },
        {
          "adapter": "unchanged_handler",
          "config": {
            "strategy": "nullify",
            "threshold_duration": "900"
          }
        }
      ]
    }
  ],
  "jobs": [
    {
      "output_id": "usdt_usd",
      "adapters": [
        {
          "adapter": "oracle_result_fetcher",
          "config": {
            "oracle_id": ".CUSDT",
            "stale_allowance": "900"
          }
        }
      ]
    },
    {
      "output_id": "bybit_mkr_usdt",
      "adapters": [
        {
          "adapter": "fetcher_multiple",
          "config": {
            "custom_node_url_key": "bybit_mkr_usdt",
            "default_node_host": "https://api-bybit.carbon.network",
            "default_node_path": "/spot/v3/public/quote/ticker/price?symbol=MKRUSDT",
            "paths": "[\"result.price\"]",
            "timeout": "10"
          }
        },
        {
          "template": "full_response_handler"
        }
      ]
    },
    {
      "output_id": "bybit_mkr_usd",
      "adapter": "math_filter",
      "config": {
        "operation": "multiply",
        "input_ids": "[\"bybit_mkr_usdt\",\"usdt_usd\"]"
      }
    },
    {
      "output_id": "binance_mkr_usdt",
      "adapters": [
        {
          "adapter": "fetcher_multiple",
          "config": {
            "custom_node_url_key": "binance_mkr_usdt",
            "default_node_host": "https://api-binance-proxy.carbon.network",
            "default_node_path": "/api/v3/ticker/price?symbol=MKRUSDT",
            "paths": "[\"price\"]",
            "timeout": "10"
          }
        },
        {
          "template": "full_response_handler"
        }
      ]
    },
    {
      "output_id": "binance_mkr_usd",
      "adapter": "math_filter",
      "config": {
        "operation": "multiply",
        "input_ids": "[\"binance_mkr_usdt\",\"usdt_usd\"]"
      }
    },
    {
      "output_id": "kraken_mkr_usd",
      "adapters": [
        {
          "adapter": "fetcher_multiple",
          "config": {
            "custom_node_url_key": "kraken_mkr_usd",
            "default_node_host": "https://api.kraken.com",
            "default_node_path": "/0/public/Ticker?pair=MKRUSD",
            "paths": "[\"result.MKRUSD.c.0\"]",
            "timeout": "10"
          }
        },
        {
          "template": "full_response_handler"
        }
      ]
    },
    {
      "adapter": "median_filter",
      "config": {
        "strategy": "nullify",
        "value_ids": "[\"bybit_mkr_usd\", \"binance_mkr_usd\", \"kraken_mkr_usd\"]",
        "tolerance_percentage": "3"
      }
    },
    {
      "output_id": "mkr_usd",
      "adapter": "weighted_average",
      "config": {
        "value_ids": "[\"bybit_mkr_usd\", \"binance_mkr_usd\", \"kraken_mkr_usd\"]",
        "weights": "[0.09, 0.87, 0.04]"
      }
    },
    {
      "output_id": "mkr_usd",
      "input_id": "mkr_usd",
      "adapter": "float_handler",
      "config": {
        "operation": "round",
        "precision": "8"
      }
    }
  ]
}