{
  "output_id": "ondo_usd",
  "templates": [
    {
      "template_id": "full_response_handler",
      "adapters": [
        {
          "adapter": "unresponsive_handler",
          "config": {
            "strategy": "use_last",
            "grace_duration": "120"
          }
        },
        {
          "adapter": "unchanged_handler",
          "config": {
            "strategy": "nullify",
            "threshold_duration": "900"
          }
        }
      ]
    }
  ],
  "jobs": [
    {
      "output_id": "usdt_usd",
      "adapters": [
        {
          "adapter": "oracle_result_fetcher",
          "config": {
            "oracle_id": ".CUSDT",
            "stale_allowance": "900"
          }
        }
      ]
    },
    {
      "output_id": "bybit_ondo_usdt",
      "adapters": [
        {
          "adapter": "fetcher_multiple",
          "config": {
            "custom_node_url_key": "bybit_ondo_usdt",
            "default_node_host": "https://api-bybit.carbon.network",
            "default_node_path": "/spot/v3/public/quote/ticker/price?symbol=ONDOUSDT",
            "paths": "[\"result.price\"]",
            "timeout": "10"
          }
        },
        {
          "template": "full_response_handler"
        }
      ]
    },
    {
      "output_id": "bybit_ondo_usd",
      "adapter": "math_filter",
      "config": {
        "operation": "multiply",
        "input_ids": "[\"bybit_ondo_usdt\",\"usdt_usd\"]"
      }
    },
    {
      "output_id": "kucoin_ondo_usdt",
      "adapters": [
        {
          "adapter": "fetcher_multiple",
          "config": {
            "custom_node_url_key": "kucoin_ondo_usdt",
            "default_node_host": "https://api.kucoin.com",
            "default_node_path": "/api/v1/market/orderbook/level1?symbol=ONDO-USDT",
            "paths": "[\"data.price\"]",
            "timeout": "10"
          }
        },
        {
          "template": "full_response_handler"
        }
      ]
    },
    {
      "output_id": "kucoin_ondo_usd",
      "adapter": "math_filter",
      "config": {
        "operation": "multiply",
        "input_ids": "[\"kucoin_ondo_usdt\",\"usdt_usd\"]"
      }
    },
    {
      "output_id": "coinbase_ondo_usd",
      "adapters": [
        {
          "adapter": "fetcher_multiple",
          "config": {
            "custom_node_url_key": "coinbase_ondo_usd",
            "default_node_host": "https://api.exchange.coinbase.com",
            "default_node_path": "/products/ONDO-USD/ticker",
            "paths": "[\"price\"]",
            "timeout": "10"
          }
        },
        {
          "template": "full_response_handler"
        }
      ]
    },
    {
      "adapter": "median_filter",
      "config": {
        "strategy": "nullify",
        "value_ids": "[\"bybit_ondo_usd\", \"kucoin_ondo_usd\", \"coinbase_ondo_usd\"]",
        "tolerance_percentage": "3"
      }
    },
    {
      "output_id": "ondo_usd",
      "adapter": "weighted_average",
      "config": {
        "value_ids": "[\"bybit_ondo_usd\", \"kucoin_ondo_usd\", \"coinbase_ondo_usd\"]",
        "weights": "[0.3, 0.3, 0.4]"
      }
    },
    {
      "output_id": "ondo_usd",
      "input_id": "ondo_usd",
      "adapter": "float_handler",
      "config": {
        "operation": "round",
        "precision": "6"
      }
    }
  ]
}