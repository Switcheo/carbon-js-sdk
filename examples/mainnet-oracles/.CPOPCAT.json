{
  "output_id": "popcat_usd",
  "templates": [
    {
      "template_id": "full_response_handler",
      "adapters": [
        {
          "adapter": "unresponsive_handler",
          "config": {
            "strategy": "use_last",
            "grace_duration": "120"
          }
        },
        {
          "adapter": "unchanged_handler",
          "config": {
            "strategy": "nullify",
            "threshold_duration": "900"
          }
        }
      ]
    }
  ],
  "jobs": [
    {
      "output_id": "usdt_usd",
      "adapters": [
        {
          "adapter": "oracle_result_fetcher",
          "config": {
            "oracle_id": ".CUSDT",
            "stale_allowance": "900"
          }
        }
      ]
    },
    {
      "output_id": "gate_popcat_usdt",
      "adapters": [
        {
          "adapter": "fetcher_multiple",
          "config": {
            "custom_node_url_key": "gate_popcat_usdt",
            "default_node_host": "https://api.gateio.ws",
            "default_node_path": "/api/v4/spot/tickers?currency_pair=POPCAT_USDT",
            "paths": "[\"0.last\"]",
            "timeout": "10"
          }
        },
        {
          "template": "full_response_handler"
        }
      ]
    },
    {
      "output_id": "gate_popcat_usd",
      "adapter": "math_filter",
      "config": {
        "operation": "multiply",
        "input_ids": "[\"gate_popcat_usdt\",\"usdt_usd\"]"
      }
    },
    {
      "output_id": "cryptodotcom_popcat_usd",
      "adapters": [
        {
          "adapter": "fetcher_multiple",
          "config": {
            "custom_node_url_key": "cryptodotcom_popcat_usd",
            "default_node_host": "https://api.crypto.com",
            "default_node_path": "/exchange/v1/public/get-tickers?instrument_name=POPCAT_USD",
            "paths": "[\"result.data.0.a\"]",
            "timeout": "10"
          }
        },
        {
          "template": "full_response_handler"
        }
      ]
    },
    {
      "output_id": "bybit_popcat_usdt",
      "adapters": [
        {
          "adapter": "fetcher_multiple",
          "config": {
            "custom_node_url_key": "bybit_popcat_usdt",
            "default_node_host": "https://api-bybit.carbon.network",
            "default_node_path": "/spot/v3/public/quote/ticker/price?symbol=POPCATUSDT",
            "paths": "[\"result.price\"]",
            "timeout": "10"
          }
        },
        {
          "template": "full_response_handler"
        }
      ]
    },
    {
      "output_id": "bybit_popcat_usd",
      "adapter": "math_filter",
      "config": {
        "operation": "multiply",
        "input_ids": "[\"bybit_popcat_usdt\",\"usdt_usd\"]"
      }
    },
    {
      "adapter": "median_filter",
      "config": {
        "strategy": "nullify",
        "value_ids": "[\"gate_popcat_usd\", \"cryptodotcom_popcat_usd\", \"bybit_popcat_usd\"]",
        "tolerance_percentage": "3"
      }
    },
    {
      "output_id": "popcat_usd",
      "adapter": "weighted_average",
      "config": {
        "value_ids": "[\"gate_popcat_usd\", \"cryptodotcom_popcat_usd\", \"bybit_popcat_usd\"]",
        "weights": "[0.51, 0.04, 0.45]"
      }
    },
    {
      "output_id": "popcat_usd",
      "input_id": "popcat_usd",
      "adapter": "float_handler",
      "config": {
        "operation": "round",
        "precision": "8"
      }
    }
  ]
}