{
  "output_id": "stride_usd",
  "templates": [
    {
      "template_id": "unchanged_response_handler",
      "adapters": [
        {
          "adapter": "unresponsive_handler",
          "config": {
            "strategy": "use_last",
            "grace_duration": "120"
          }
        },
        {
          "adapter": "unchanged_handler",
          "config": {
            "strategy": "nullify",
            "threshold_duration": "1800"
          }
        }
      ]
    },
    {
      "template_id": "rpc_response_handler",
      "adapters": [
        {
          "adapter": "unresponsive_handler",
          "config": {
            "strategy": "use_last",
            "grace_duration": "120"
          }
        }
      ]
    }
  ],
  "jobs": [
    {
      "output_id": "osmosis_stride_osmo_pool",
      "adapters": [
        {
          "adapter": "fetcher_multiple",
          "config": {
            "custom_node_url_key": "osmosis_806_3",
            "default_node_host": "https://api-osmosis.carbon.network",
            "default_node_path": "/osmosis/gamm/v1beta1/pools/806",
            "paths": "[\"pool.pool_assets.1.token.amount\", \"pool.pool_assets.0.token.amount\"]",
            "timeout": "10"
          }
        },
        {
          "template": "unchanged_response_handler"
        }
      ]
    },
    {
      "output_id": "osmosis_stride_osmo",
      "adapter": "math_filter",
      "input_id": "osmosis_stride_osmo_pool",
      "config": {
        "operation": "divide"
      }
    },
    {
      "output_id": "osmo_usd",
      "adapters": [
        {
          "adapter": "oracle_result_fetcher",
          "config": {
            "oracle_id": ".COSMO",
            "stale_allowance": "900"
          }
        }
      ]
    },
    {
      "output_id": "stride_usd",
      "adapter": "math_filter",
      "config": {
        "operation": "multiply",
        "input_ids": "[\"osmosis_stride_osmo\",\"osmo_usd\"]"
      }
    },
    {
      "output_id": "stride_usd",
      "input_id": "stride_usd",
      "adapter": "float_handler",
      "config": {
        "operation": "round",
        "precision": "6"
      }
    }
  ]
}