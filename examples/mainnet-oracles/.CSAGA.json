{
  "output_id": "saga_usd",
  "templates": [
    {
      "template_id": "full_response_handler",
      "adapters": [
        {
          "adapter": "unresponsive_handler",
          "config": {
            "strategy": "use_last",
            "grace_duration": "120"
          }
        },
        {
          "adapter": "unchanged_handler",
          "config": {
            "strategy": "nullify",
            "threshold_duration": "900"
          }
        }
      ]
    }
  ],
  "jobs": [
    {
      "output_id": "usdt_usd",
      "adapters": [
        {
          "adapter": "oracle_result_fetcher",
          "config": {
            "oracle_id": ".CUSDT",
            "stale_allowance": "900"
          }
        }
      ]
    },
    {
      "output_id": "bybit_saga_usdt",
      "adapters": [
        {
          "adapter": "fetcher_multiple",
          "config": {
            "custom_node_url_key": "bybit_perps_saga_usdt",
            "default_node_host": "https://api-bybit.carbon.network",
            "default_node_path": "/derivatives/v3/public/tickers?category=linear&symbol=SAGAUSDT",
            "paths": "[\"result.list.0.indexPrice\"]",
            "timeout": "10"
          }
        },
        {
          "template": "full_response_handler"
        }
      ]
    },
    {
      "output_id": "bybit_saga_usd",
      "adapter": "math_filter",
      "config": {
        "operation": "multiply",
        "input_ids": "[\"bybit_saga_usdt\",\"usdt_usd\"]"
      }
    },
    {
      "output_id": "binance_saga_usdt",
      "adapters": [
        {
          "adapter": "fetcher_multiple",
          "config": {
            "custom_node_url_key": "binance_saga_usdt",
            "default_node_host": "https://api-binance-proxy.carbon.network",
            "default_node_path": "/api/v3/ticker/price?symbol=SAGAUSDT",
            "paths": "[\"price\"]",
            "timeout": "10"
          }
        },
        {
          "template": "full_response_handler"
        }
      ]
    },
    {
      "output_id": "binance_saga_usd",
      "adapter": "math_filter",
      "config": {
        "operation": "multiply",
        "input_ids": "[\"binance_saga_usdt\",\"usdt_usd\"]"
      }
    },
    {
      "output_id": "kraken_saga_usd",
      "adapters": [
        {
          "adapter": "fetcher_multiple",
          "config": {
            "custom_node_url_key": "kraken_saga_usd",
            "default_node_host": "https://api.kraken.com",
            "default_node_path": "/0/public/Ticker?pair=SAGAUSD",
            "paths": "[\"result.SAGAUSD.c.0\"]",
            "timeout": "10"
          }
        },
        {
          "template": "full_response_handler"
        }
      ]
    },
    {
      "adapter": "median_filter",
      "config": {
        "strategy": "nullify",
        "value_ids": "[\"bybit_saga_usd\", \"binance_saga_usd\", \"kraken_saga_usd\"]",
        "tolerance_percentage": "3"
      }
    },
    {
      "output_id": "saga_usd",
      "adapter": "weighted_average",
      "config": {
        "value_ids": "[\"bybit_saga_usd\", \"binance_saga_usd\", \"kraken_saga_usd\"]",
        "weights": "[0.15, 0.70, 0.15]"
      }
    },
    {
      "output_id": "saga_usd",
      "input_id": "saga_usd",
      "adapter": "float_handler",
      "config": {
        "operation": "round",
        "precision": "8"
      }
    }
  ]
}