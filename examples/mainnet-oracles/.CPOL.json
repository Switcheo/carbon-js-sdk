{
  "output_id": "pol_usd",
  "templates": [
    {
      "template_id": "full_response_handler",
      "adapters": [
        {
          "adapter": "unresponsive_handler",
          "config": {
            "strategy": "use_last",
            "grace_duration": "120"
          }
        },
        {
          "adapter": "unchanged_handler",
          "config": {
            "strategy": "nullify",
            "threshold_duration": "900"
          }
        }
      ]
    }
  ],
  "jobs": [
    {
      "output_id": "usdt_usd",
      "adapters": [
        {
          "adapter": "oracle_result_fetcher",
          "config": {
            "oracle_id": ".CUSDT",
            "stale_allowance": "900"
          }
        }
      ]
    },
    {
      "output_id": "bybit_pol_usdt",
      "adapters": [
        {
          "adapter": "fetcher_multiple",
          "config": {
            "custom_node_url_key": "bybit_pol_usdt",
            "default_node_host": "https://api-bybit.carbon.network",
            "default_node_path": "/v5/market/tickers?category=spot&symbol=POLUSDT",
            "paths": "[\"result.list.0.lastPrice\"]",
            "timeout": "10"
          }
        },
        {
          "template": "full_response_handler"
        }
      ]
    },
    {
      "output_id": "bybit_pol_usd",
      "adapter": "math_filter",
      "config": {
        "operation": "multiply",
        "input_ids": "[\"bybit_pol_usdt\",\"usdt_usd\"]"
      }
    },
    {
      "output_id": "binance_pol_usdt",
      "adapters": [
        {
          "adapter": "fetcher_multiple",
          "config": {
            "custom_node_url_key": "binance_pol_usdt",
            "default_node_host": "https://api-binance-proxy-1.carbon.network",
            "default_node_path": "/api/v3/ticker/price?symbol=POLUSDT",
            "paths": "[\"price\"]",
            "timeout": "10"
          }
        },
        {
          "template": "full_response_handler"
        }
      ]
    },
    {
      "output_id": "binance_pol_usd",
      "adapter": "math_filter",
      "config": {
        "operation": "multiply",
        "input_ids": "[\"binance_pol_usdt\",\"usdt_usd\"]"
      }
    },
    {
      "output_id": "coinbase_pol_usd",
      "adapters": [
        {
          "adapter": "fetcher_multiple",
          "config": {
            "custom_node_url_key": "coinbase_pol_usd",
            "default_node_host": "https://api.exchange.coinbase.com",
            "default_node_path": "/products/POL-USD/ticker",
            "paths": "[\"price\"]",
            "timeout": "10"
          }
        },
        {
          "template": "full_response_handler"
        }
      ]
    },
    {
      "adapter": "median_filter",
      "config": {
        "strategy": "nullify",
        "value_ids": "[\"bybit_pol_usd\", \"binance_pol_usd\", \"coinbase_pol_usd\"]",
        "tolerance_percentage": "3"
      }
    },
    {
      "output_id": "pol_usd",
      "adapter": "weighted_average",
      "config": {
        "value_ids": "[\"bybit_pol_usd\", \"binance_pol_usd\", \"coinbase_pol_usd\"]",
        "weights": "[0.10, 0.80, 0.10]"
      }
    },
    {
      "output_id": "pol_usd",
      "input_id": "pol_usd",
      "adapter": "float_handler",
      "config": {
        "operation": "round",
        "precision": "8"
      }
    }
  ]
}