{
  "output_id": "ststars_usd",
  "templates": [
    {
      "template_id": "rpc_response_handler",
      "adapters": [
        {
          "adapter": "unresponsive_handler",
          "config": {
            "strategy": "use_last",
            "grace_duration": "120"
          }
        }
      ]
    }
  ],
  "jobs": [
    {
      "output_id": "stars_usd",
      "adapters": [
        {
          "adapter": "oracle_result_fetcher",
          "config": {
            "oracle_id": ".CSTARS",
            "stale_allowance": "900"
          }
        }
      ]
    },
    {
      "output_id": "ststars_stride_redemption_rate",
      "adapters": [
        {
          "adapter": "fetcher_multiple",
          "config": {
            "custom_node_url_key": "stride_ststars_redemption_rate",
            "default_node_host": "https://stride-rest.publicnode.com",
            "default_node_path": "/Stride-Labs/stride/stakeibc/host_zone/stargaze-1",
            "paths": "[\"host_zone.redemption_rate\"]",
            "timeout": "10"
          }
        },
        {
          "template": "rpc_response_handler"
        }
      ]
    },
    {
      "output_id": "ststars_stars_swap_rate",
      "adapters": [
        {
          "adapter": "fetcher_multiple",
          "config": {
            "custom_node_url_key": "ststars_stars_swap_rate_1",
            "default_node_host": "https://api-osmosis.carbon.network",
            "default_node_path": "/osmosis/gamm/v1beta1/pools/810/prices?base_asset_denom=ibc/987C17B11ABC2B20019178ACE62929FE9840202CE79498E29FE8E5CB02B7C0A4&quote_asset_denom=ibc/5DD1F95ED336014D00CE2520977EC71566D282F9749170ADC83A392E0EA7426A",
            "paths": "[\"spot_price\"]",
            "timeout": "10"
          }
        },
        {
          "template": "rpc_response_handler"
        }
      ]
    },
    {
      "output_id": "ststars_redemption_rate",
      "adapter": "math_filter",
      "input_id": "",
      "config": {
        "operation": "min",
        "input_ids": "[\"ststars_stars_swap_rate\",\"ststars_stride_redemption_rate\"]"
      }
    },
    {
      "output_id": "ststars_usd",
      "adapter": "math_filter",
      "config": {
        "operation": "multiply",
        "input_ids": "[\"ststars_redemption_rate\",\"stars_usd\"]"
      }
    },
    {
      "output_id": "ststars_usd",
      "input_id": "ststars_usd",
      "adapter": "float_handler",
      "config": {
        "operation": "round",
        "precision": "6"
      }
    },
    {
      "output_id": "ststars_usd",
      "input_id": "ststars_usd",
      "adapter": "unchanged_handler",
      "config": {
        "strategy": "nullify",
        "threshold_duration": "900"
      }
    }
  ]
}