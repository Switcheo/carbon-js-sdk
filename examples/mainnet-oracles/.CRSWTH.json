{
    "output_id": "rswth_usd",
    "templates": [
      {
        "template_id": "response_handler",
        "adapters": [
          {
            "adapter": "unresponsive_handler",
            "config": {
              "strategy": "use_last",
              "grace_duration": "10"
            }
          }
        ]
      }
    ],
    "jobs": [
      {
        "output_id": "stafihub_rswth_redemption_rate",
        "adapters": [
          {
            "adapter": "fetcher_multiple",
            "config": {
              "custom_node_url_key": "stafihub_rswth_redemption_rate",
              "default_node_host": "https://public-rest-rpc1.stafihub.io",
              "default_node_path": "/stafihub/stafihub/ledger/exchange_rate/urswth",
              "paths": "[\"exchangeRate.value\"]",
              "timeout": "10"
            }
          },
          {
            "template": "response_handler"
          }
        ]
      },
      {
        "output_id": "swth_usd",
        "adapters": [
          {
            "adapter": "oracle_result_fetcher",
            "config": {
                "oracle_id": ".CSWTH",
                "stale_allowance": "14400"
            }
          }
        ]
      },
      {
        "output_id": "rswth_usd",
        "adapter": "math_filter",
        "config": {
          "operation": "multiply",
          "input_ids": "[\"stafihub_rswth_redemption_rate\",\"swth_usd\"]"
        }
      },
      {
        "output_id": "rswth_usd",
        "input_id": "rswth_usd",
        "adapter": "unchanged_handler",
        "config": {
          "strategy": "nullify",
          "threshold_duration": "14400"
        }
      },
      {
        "output_id": "rswth_usd",
        "input_id": "rswth_usd",
        "adapter": "float_handler",
        "config": {
          "operation": "round",
          "precision": "8"
        }
      }
    ]
  }