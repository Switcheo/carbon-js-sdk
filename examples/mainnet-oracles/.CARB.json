{
  "output_id": "arb_usd",
  "templates": [
    {
      "template_id": "response_handler",
      "adapters": [
        {
          "adapter": "unresponsive_handler",
          "config": {
            "strategy": "use_last",
            "grace_duration": "120"
          }
        },
        {
          "adapter": "unchanged_handler",
          "config": {
            "strategy": "nullify",
            "threshold_duration": "900"
          }
        }
      ]
    }
  ],
  "jobs": [
    {
      "output_id": "binance_arb_usdt",
      "adapters": [
        {
          "adapter": "fetcher_multiple",
          "config": {
            "custom_node_url_key": "binance_arb_usdt",
            "default_node_host": "https://api-binance-proxy.carbon.network",
            "default_node_path": "/api/v3/ticker/price?symbol=ARBUSDT",
            "paths": "[\"price\"]",
            "timeout": "10"
          }
        },
        {
          "template": "response_handler"
        }
      ]
    },
    {
      "output_id": "usdt_usd",
      "adapters": [
        {
          "adapter": "oracle_result_fetcher",
          "config": {
            "oracle_id": ".CUSDT",
            "stale_allowance": "900"
          }
        }
      ]
    },
    {
      "output_id": "binance_arb_usd",
      "adapter": "math_filter",
      "config": {
        "operation": "multiply",
        "input_ids": "[\"binance_arb_usdt\",\"usdt_usd\"]"
      }
    },
    {
      "output_id": "okx_arb_usdt",
      "adapters": [
        {
          "adapter": "fetcher_multiple",
          "config": {
            "custom_node_url_key": "okx_arb_usdt",
            "default_node_host": "https://www.okx.com",
            "default_node_path": "/api/v5/market/index-tickers?instId=ARB-USDT",
            "paths": "[\"data.0.idxPx\"]",
            "timeout": "10"
          }
        },
        {
          "template": "response_handler"
        }
      ]
    },
    {
      "output_id": "okx_arb_usd",
      "adapter": "math_filter",
      "config": {
        "operation": "multiply",
        "input_ids": "[\"okx_arb_usdt\",\"usdt_usd\"]"
      }
    },
    {
      "output_id": "coinbase_arb_usd",
      "adapters": [
        {
          "adapter": "fetcher_multiple",
          "config": {
            "custom_node_url_key": "coinbase_arb_usd",
            "default_node_host": "https://api.exchange.coinbase.com",
            "default_node_path": "/products/ARB-USD/ticker",
            "paths": "[\"price\"]",
            "timeout": "10"
          }
        },
        {
          "template": "response_handler"
        }
      ]
    },
    {
      "adapter": "median_filter",
      "config": {
        "strategy": "nullify",
        "value_ids": "[\"binance_arb_usd\", \"okx_arb_usd\",\"coinbase_arb_usd\"]",
        "tolerance_percentage": "3"
      }
    },
    {
      "output_id": "arb_usd",
      "adapter": "weighted_average",
      "config": {
        "value_ids": "[\"binance_arb_usd\",\"okx_arb_usd\",\"coinbase_arb_usd\"]",
        "weights": "[0.4, 0.3, 0.3]"
      }
    },
    {
      "output_id": "arb_usd",
      "input_id": "arb_usd",
      "adapter": "float_handler",
      "config": {
        "operation": "round",
        "precision": "8"
      }
    }
  ]
}