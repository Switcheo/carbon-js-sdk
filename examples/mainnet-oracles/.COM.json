{
  "output_id": "om_usd",
  "templates": [
    {
      "template_id": "full_response_handler",
      "adapters": [
        {
          "adapter": "unresponsive_handler",
          "config": {
            "strategy": "use_last",
            "grace_duration": "120"
          }
        },
        {
          "adapter": "unchanged_handler",
          "config": {
            "strategy": "nullify",
            "threshold_duration": "900"
          }
        }
      ]
    }
  ],
  "jobs": [
    {
      "output_id": "binance_om_usdt",
      "adapters": [
        {
          "adapter": "fetcher_multiple",
          "config": {
            "custom_node_url_key": "binance_om_usdt",
            "default_node_host": "https://api-binance-proxy-1.carbon.network",
            "default_node_path": "/api/v3/ticker/price?symbol=OMUSDT",
            "paths": "[\"price\"]",
            "timeout": "10"
          }
        },
        {
          "template": "full_response_handler"
        }
      ]
    },
    {
      "output_id": "okx_om_usdt",
      "adapters": [
        {
          "adapter": "fetcher_multiple",
          "config": {
            "custom_node_url_key": "okx_om_usdt",
            "default_node_host": "https://www.okx.com",
            "default_node_path": "/api/v5/market/index-tickers?instId=OM-USDT",
            "paths": "[\"data.0.idxPx\"]",
            "timeout": "10"
          }
        },
        {
          "template": "full_response_handler"
        }
      ]
    },
    {
      "output_id": "kucoin_om_usdt",
      "adapters": [
        {
          "adapter": "fetcher_multiple",
          "config": {
            "custom_node_url_key": "kucoin_om_usdt",
            "default_node_host": "https://api.kucoin.com",
            "default_node_path": "/api/v1/market/orderbook/level1?symbol=OM-USDT",
            "paths": "[\"data.price\"]",
            "timeout": "10"
          }
        },
        {
          "template": "full_response_handler"
        }
      ]
    },
    {
      "adapter": "median_filter",
      "config": {
        "strategy": "nullify",
        "value_ids": "[\"binance_om_usdt\", \"okx_om_usdt\", \"kucoin_om_usdt\"]",
        "tolerance_percentage": "3"
      }
    },
    {
      "output_id": "om_usdt",
      "adapter": "weighted_average",
      "config": {
        "value_ids": "[\"binance_om_usdt\", \"okx_om_usdt\", \"kucoin_om_usdt\"]",
        "weights": "[0.70, 0.15, 0.15]"
      }
    },
    {
      "output_id": "usdt_usd",
      "adapters": [
        {
          "adapter": "oracle_result_fetcher",
          "config": {
            "oracle_id": ".CUSDT",
            "stale_allowance": "900"
          }
        }
      ]
    },
    {
      "output_id": "om_usd",
      "adapter": "math_filter",
      "config": {
        "operation": "multiply",
        "input_ids": "[\"om_usdt\",\"usdt_usd\"]"
      }
    },
    {
      "output_id": "om_usd",
      "input_id": "om_usd",
      "adapter": "float_handler",
      "config": {
        "operation": "round",
        "precision": "8"
      }
    }
  ]
}