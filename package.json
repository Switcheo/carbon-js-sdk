{
  "name": "carbon-sdk",
  "version": "0.0.1",
  "description": "TypeScript SDK for Carbon blockchain",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "repository": "https://github.com/Switcheo/carbon-sdk",
  "author": "Switcheo <engineering@switcheo.network>",
  "license": "MIT",
  "scripts": {
    "build": "tsc && tsc-alias",
    "sync-proto": "sh scripts/update-proto.sh",
    "test": "tsc && node lib/test.js",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "tslint -p tsconfig.json",
    "prepare": "yarn run build",
    "prepublishOnly": "yarn run lint",
    "preversion": "yarn run lint",
    "version": "yarn run format && git add -A src",
    "postversion": "git push && git push --tags",
    "ws": "^7.4.6"
  },
  "files": [
    "lib/**/*",
    "examples/**/*"
  ],
  "dependencies": {
    "@cosmjs/proto-signing": "^0.25.4",
    "@cosmjs/stargate": "^0.25.4",
    "@grpc/grpc-js": "^1.3.2",
    "@types/ws": "^7.4.7",
    "base58check": "^2.0.0",
    "bignumber.js": "^9.0.1",
    "bip32": "^2.0.6",
    "bip39": "^3.0.4",
    "cosmjs-types": "^0.1.0",
    "dayjs": "^1.10.5",
    "ethers": "^5.3.0",
    "secp256k1": "^4.0.2",
    "secp256r1": "^0.0.3",
    "tsc-alias": "^1.2.11",
    "typescript": "^4.3.5",
    "wif": "^2.0.6"
  },
  "devDependencies": {
    "@types/node": "^15.6.2",
    "@types/secp256k1": "^4.0.2",
    "@types/wif": "^2.0.2",
    "dotenv": "^10.0.0",
    "ts-proto": "^1.81.1"
  }
}
