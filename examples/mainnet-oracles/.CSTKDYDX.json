{
  "output_id": "stkdydx_usd",
  "templates": [
    {
      "template_id": "rpc_response_handler",
      "adapters": [
        {
          "adapter": "unresponsive_handler",
          "config": {
            "strategy": "use_last",
            "grace_duration": "120"
          }
        }
      ]
    }
  ],
  "jobs": [
    {
      "output_id": "dydx_usd",
      "adapters": [
        {
          "adapter": "oracle_result_fetcher",
          "config": {
            "oracle_id": ".CDYDX",
            "stale_allowance": "900"
          }
        }
      ]
    },
    {
      "output_id": "persistence_stkdydx_redemption_rate_inverse",
      "adapters": [
        {
          "adapter": "fetcher_multiple",
          "config": {
            "custom_node_url_key": "persistence_stkdydx_dydx_redemption_rate",
            "default_node_host": "https://oracle-utils.carbon.network",
            "default_node_path": "/stkdydx-redemption-rate",
            "paths": "[\"price\"]",
            "timeout": "10"
          }
        },
        {
          "template": "rpc_response_handler"
        }
      ]
    },
    {
      "adapter": "static_handler",
      "output_id": "static_one",
      "config": {
        "value": "1"
      }
    },
    {
      "output_id": "persistence_stkdydx_redemption_rate",
      "adapter": "math_filter",
      "config": {
        "operation": "divide",
        "input_ids": "[\"static_one\",\"persistence_stkdydx_redemption_rate_inverse\"]"
      }
    },
    {
      "output_id": "dexter_stkdydx_dydx_swap_rate_18",
      "adapters": [
        {
          "adapter": "fetcher_multiple",
          "config": {
            "custom_node_url_key": "dexter_stkdydx_dydx_swap_rate",
            "default_node_host": "https://rest-persistence.architectnodes.com",
            "default_node_path": "",
            "paths": "[\"data.trade_params.amount_out\"]",
            "timeout": "10"
          }
        },
        {
          "template": "rpc_response_handler"
        }
      ]
    },
    {
      "output_id": "dexter_stkdydx_dydx_swap_rate",
      "input_id": "dexter_stkdydx_dydx_swap_rate_18",
      "adapter": "decimal_handler",
      "config": {
        "operation": "decrease",
        "exponent": "18"
      }
    },
    {
      "output_id": "stkdydx_redemption_rate",
      "adapter": "math_filter",
      "input_id": "",
      "config": {
        "operation": "min",
        "input_ids": "[\"dexter_stkdydx_dydx_swap_rate\",\"persistence_stkdydx_redemption_rate\"]"
      }
    },
    {
      "output_id": "stkdydx_usd",
      "adapter": "math_filter",
      "config": {
        "operation": "multiply",
        "input_ids": "[\"stkdydx_redemption_rate\",\"dydx_usd\"]"
      }
    },
    {
      "output_id": "stkdydx_usd",
      "input_id": "stkdydx_usd",
      "adapter": "float_handler",
      "config": {
        "operation": "round",
        "precision": "6"
      }
    },
    {
      "output_id": "stkdydx_usd",
      "input_id": "stkdydx_usd",
      "adapter": "unchanged_handler",
      "config": {
        "strategy": "nullify",
        "threshold_duration": "900"
      }
    }
  ]
}