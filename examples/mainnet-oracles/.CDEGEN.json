{
  "output_id": "degen_usd",
  "templates": [
    {
      "template_id": "full_response_handler",
      "adapters": [
        {
          "adapter": "unresponsive_handler",
          "config": {
            "strategy": "use_last",
            "grace_duration": "120"
          }
        },
        {
          "adapter": "unchanged_handler",
          "config": {
            "strategy": "nullify",
            "threshold_duration": "900"
          }
        }
      ]
    }
  ],
  "jobs": [
    {
      "output_id": "bybit_degen_usdt",
      "adapters": [
        {
          "adapter": "fetcher_multiple",
          "config": {
            "custom_node_url_key": "bybit_perps_degen_usdt",
            "default_node_host": "https://api.bybit.com",
            "default_node_path": "/spot/v3/public/quote/ticker/price?symbol=DEGENUSDT",
            "paths": "[\"result.price\"]",
            "timeout": "10"
          }
        },
        {
          "template": "full_response_handler"
        }
      ]
    },
    {
      "output_id": "kucoin_degen_usdt",
      "adapters": [
        {
          "adapter": "fetcher_multiple",
          "config": {
            "custom_node_url_key": "kucoin_degen_usdt",
            "default_node_host": "https://api.kucoin.com",
            "default_node_path": "/api/v1/market/orderbook/level1?symbol=DEGEN-USDT",
            "paths": "[\"data.price\"]",
            "timeout": "10"
          }
        },
        {
          "template": "full_response_handler"
        }
      ]
    },
    {
      "output_id": "htx_degen_usdt",
      "adapters": [
        {
          "adapter": "fetcher_multiple",
          "config": {
            "custom_node_url_key": "htx_degen_usdt",
            "default_node_host": "https://api.huobi.pro",
            "default_node_path": "/market/trade?symbol=degenusdt",
            "paths": "[\"tick.data.0.price\"]",
            "timeout": "10"
          }
        },
        {
          "template": "full_response_handler"
        }
      ]
    },
    {
      "adapter": "median_filter",
      "config": {
        "strategy": "nullify",
        "value_ids": "[\"bybit_degen_usdt\", \"kucoin_degen_usdt\", \"htx_degen_usdt\"]",
        "tolerance_percentage": "3"
      }
    },
    {
      "output_id": "degen_usdt",
      "adapter": "weighted_average",
      "config": {
        "value_ids": "[\"bybit_degen_usdt\", \"kucoin_degen_usdt\", \"htx_degen_usdt\"]",
        "weights": "[0.5, 0.2, 0.3]"
      }
    },
    {
      "output_id": "usdt_usd",
      "adapters": [
        {
          "adapter": "oracle_result_fetcher",
          "config": {
            "oracle_id": ".CUSDT",
            "stale_allowance": "900"
          }
        }
      ]
    },
    {
      "output_id": "degen_usd",
      "adapter": "math_filter",
      "config": {
        "operation": "multiply",
        "input_ids": "[\"degen_usdt\",\"usdt_usd\"]"
      }
    },
    {
      "output_id": "degen_usd",
      "input_id": "degen_usd",
      "adapter": "float_handler",
      "config": {
        "operation": "round",
        "precision": "6"
      }
    }
  ]
}