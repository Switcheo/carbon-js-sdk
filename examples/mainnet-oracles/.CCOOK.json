{
  "output_id": "cook_usd",
  "templates": [
    {
      "template_id": "full_response_handler",
      "adapters": [
        {
          "adapter": "unresponsive_handler",
          "config": {
            "strategy": "use_last",
            "grace_duration": "120"
          }
        },
        {
          "adapter": "unchanged_handler",
          "config": {
            "strategy": "nullify",
            "threshold_duration": "900"
          }
        }
      ]
    }
  ],
  "jobs": [
    {
      "output_id": "bybit_cook_usdt",
      "adapters": [
        {
          "adapter": "fetcher_multiple",
          "config": {
            "custom_node_url_key": "bybit_cook_usdt",
            "default_node_host": "https://api-bybit.carbon.network",
            "default_node_path": "/v5/market/tickers?category=spot&symbol=COOKUSDT",
            "paths": "[\"result.list.0.lastPrice\"]",
            "timeout": "10"
          }
        },
        {
          "template": "full_response_handler"
        }
      ]
    },
    {
      "output_id": "usdt_usd",
      "adapters": [
        {
          "adapter": "oracle_result_fetcher",
          "config": {
            "oracle_id": ".CUSDT",
            "stale_allowance": "900"
          }
        }
      ]
    },
    {
      "output_id": "cook_usd",
      "adapter": "math_filter",
      "config": {
        "operation": "multiply",
        "input_ids": "[\"bybit_cook_usdt\",\"usdt_usd\"]"
      }
    },
    {
      "output_id": "cook_usd",
      "input_id": "cook_usd",
      "adapter": "float_handler",
      "config": {
        "operation": "round",
        "precision": "8"
      }
    }
  ]
}