{
  "output_id": "usdt_usd",
  "templates": [
    {
      "template_id": "response_handler",
      "adapters": [
        {
          "adapter": "unresponsive_handler",
          "config": {
            "strategy": "use_last",
            "grace_duration": "120"
          }
        },
        {
          "adapter": "unchanged_handler",
          "config": {
            "strategy": "nullify",
            "threshold_duration": "900"
          }
        }
      ]
    }
  ],
  "jobs": [
    {
      "output_id": "kraken_usdt_usd",
      "adapters": [
        {
          "adapter": "fetcher_multiple",
          "config": {
            "custom_node_url_key": "kraken_usdt_usd",
            "default_node_host": "https://api.kraken.com",
            "default_node_path": "/0/public/Ticker?pair=USDTUSD",
            "paths": "[\"result.USDTZUSD.c.0\"]",
            "timeout": "10"
          }
        },
        {
          "template": "response_handler"
        }
      ]
    },
    {
      "output_id": "coinbase_usdt_usd",
      "adapters": [
        {
          "adapter": "fetcher_multiple",
          "config": {
            "custom_node_url_key": "coinbase_usdt_usd",
            "default_node_host": "https://api.exchange.coinbase.com",
            "default_node_path": "/products/USDT-USD/ticker",
            "paths": "[\"price\"]",
            "timeout": "10"
          }
        },
        {
          "template": "response_handler"
        }
      ]
    },
    {
      "output_id": "bitfinex_usdt_usd",
      "adapters": [
        {
          "adapter": "fetcher_multiple",
          "config": {
            "custom_node_url_key": "bitfinex_usdt_usd",
            "default_node_host": "https://api.bitfinex.com",
            "default_node_path": "/v1/pubticker/ustusd",
            "paths": "[\"mid\"]",
            "timeout": "10"
          }
        },
        {
          "template": "response_handler"
        }
      ]
    },
    {
      "output_id": "kraken_usdt_usd",
      "input_id": "kraken_usdt_usd",
      "adapter": "median_filter",
      "config": {
        "strategy": "nullify",
        "value_ids": "[\"kraken_usdt_usd\", \"coinbase_usdt_usd\", \"bitfinex_usdt_usd\"]",
        "tolerance_percentage": "1"
      }
    },
    {
      "output_id": "coinbase_usdt_usd",
      "input_id": "coinbase_usdt_usd",
      "adapter": "median_filter",
      "config": {
        "strategy": "nullify",
        "value_ids": "[\"kraken_usdt_usd\", \"coinbase_usdt_usd\", \"bitfinex_usdt_usd\"]",
        "tolerance_percentage": "1"
      }
    },
    {
      "output_id": "bitfinex_usdt_usd",
      "input_id": "bitfinex_usdt_usd",
      "adapter": "median_filter",
      "config": {
        "strategy": "nullify",
        "value_ids": "[\"kraken_usdt_usd\", \"coinbase_usdt_usd\", \"bitfinex_usdt_usd\"]",
        "tolerance_percentage": "1"
      }
    },
    {
      "output_id": "usdt_usd",
      "adapter": "weighted_average",
      "config": {
        "value_ids": "[\"kraken_usdt_usd\", \"coinbase_usdt_usd\", \"bitfinex_usdt_usd\"]",
        "weights": "[0.4, 0.4, 0.2]"
      }
    },
    {
      "adapter": "static_handler",
      "output_id": "static_one",
      "config": {
        "value": "1.15"
      }
    },
    {
      "output_id": "usdt_usd",
      "adapter": "math_filter",
      "input_id": "",
      "config": {
        "operation": "min",
        "input_ids": "[\"usdt_usd\",\"static_one\"]"
      }
    },
    {
      "output_id": "usdt_usd",
      "input_id": "usdt_usd",
      "adapter": "float_handler",
      "config": {
        "operation": "round",
        "precision": "6"
      }
    }
  ]
}
