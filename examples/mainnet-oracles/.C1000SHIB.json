{
  "output_id": "shib_usd",
  "templates": [
    {
      "template_id": "full_response_handler",
      "adapters": [
        {
          "adapter": "unresponsive_handler",
          "config": {
            "strategy": "use_last",
            "grace_duration": "120"
          }
        },
        {
          "adapter": "unchanged_handler",
          "config": {
            "strategy": "nullify",
            "threshold_duration": "900"
          }
        }
      ]
    }
  ],
  "jobs": [
    {
      "output_id": "binance_shib_usdt",
      "adapters": [
        {
          "adapter": "fetcher_multiple",
          "config": {
            "custom_node_url_key": "binance_shib_usdt",
            "default_node_host": "https://api-binance-proxy.carbon.network",
            "default_node_path": "/api/v3/ticker/price?symbol=SHIBUSDT",
            "paths": "[\"price\"]",
            "timeout": "10"
          }
        },
        {
          "template": "full_response_handler"
        }
      ]
    },
    {
      "output_id": "usdt_usd",
      "adapters": [
        {
          "adapter": "oracle_result_fetcher",
          "config": {
            "oracle_id": ".CUSDT",
            "stale_allowance": "900"
          }
        }
      ]
    },
    {
      "output_id": "binance_shib_usd",
      "adapter": "math_filter",
      "config": {
        "operation": "multiply",
        "input_ids": "[\"binance_shib_usdt\",\"usdt_usd\"]"
      }
    },
    {
      "output_id": "kraken_shib_usd",
      "adapters": [
        {
          "adapter": "fetcher_multiple",
          "config": {
            "custom_node_url_key": "kraken_shib_usd",
            "default_node_host": "https://api.kraken.com",
            "default_node_path": "/0/public/Ticker?pair=SHIBUSD",
            "paths": "[\"result.SHIBUSD.c.0\"]",
            "timeout": "10"
          }
        },
        {
          "template": "full_response_handler"
        }
      ]
    },
    {
      "output_id": "coinbase_shib_usd",
      "adapters": [
        {
          "adapter": "fetcher_multiple",
          "config": {
            "custom_node_url_key": "coinbase_shib_usd",
            "default_node_host": "https://api.exchange.coinbase.com",
            "default_node_path": "/products/SHIB-USD/ticker",
            "paths": "[\"price\"]",
            "timeout": "10"
          }
        },
        {
          "template": "full_response_handler"
        }
      ]
    },
    {
      "output_id": "binance_shib_usd",
      "input_id": "binance_shib_usd",
      "adapter": "decimal_handler",
      "config": {
        "operation": "increase",
        "exponent": "6"
      }
    },
    {
      "output_id": "kraken_shib_usd",
      "input_id": "kraken_shib_usd",
      "adapter": "decimal_handler",
      "config": {
        "operation": "increase",
        "exponent": "6"
      }
    },
    {
      "output_id": "coinbase_shib_usd",
      "input_id": "coinbase_shib_usd",
      "adapter": "decimal_handler",
      "config": {
        "operation": "increase",
        "exponent": "6"
      }
    },
    {
      "adapter": "median_filter",
      "config": {
        "strategy": "nullify",
        "value_ids": "[\"binance_shib_usd\", \"kraken_shib_usd\",\"coinbase_shib_usd\"]",
        "tolerance_percentage": "3"
      }
    },
    {
      "output_id": "shib_usd",
      "adapter": "weighted_average",
      "config": {
        "value_ids": "[\"binance_shib_usd\",\"kraken_shib_usd\",\"coinbase_shib_usd\"]",
        "weights": "[0.51, 0.17, 0.32]"
      }
    },
    {
      "output_id": "shib_usd",
      "input_id": "shib_usd",
      "adapter": "decimal_handler",
      "config": {
        "operation": "decrease",
        "exponent": "3"
      }
    },
    {
      "output_id": "shib_usd",
      "input_id": "shib_usd",
      "adapter": "float_handler",
      "config": {
        "operation": "round",
        "precision": "8"
      }
    }
  ]
}