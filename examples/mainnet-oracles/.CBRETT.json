{
  "output_id": "brett_usd",
  "templates": [
    {
      "template_id": "full_response_handler",
      "adapters": [
        {
          "adapter": "unresponsive_handler",
          "config": {
            "strategy": "use_last",
            "grace_duration": "120"
          }
        },
        {
          "adapter": "unchanged_handler",
          "config": {
            "strategy": "nullify",
            "threshold_duration": "900"
          }
        }
      ]
    }
  ],
  "jobs": [
    {
      "output_id": "bybit_brett_usdt",
      "adapters": [
        {
          "adapter": "fetcher_multiple",
          "config": {
            "custom_node_url_key": "bybit_perps_brett_usdt",
            "default_node_host": "https://api.bybit.com",
            "default_node_path": "/spot/v3/public/quote/ticker/price?symbol=BRETTUSDT",
            "paths": "[\"result.price\"]",
            "timeout": "10"
          }
        },
        {
          "template": "full_response_handler"
        }
      ]
    },
    {
      "output_id": "kucoin_brett_usdt",
      "adapters": [
        {
          "adapter": "fetcher_multiple",
          "config": {
            "custom_node_url_key": "kucoin_brett_usdt",
            "default_node_host": "https://api.kucoin.com",
            "default_node_path": "/api/v1/market/orderbook/level1?symbol=BRETT-USDT",
            "paths": "[\"data.price\"]",
            "timeout": "10"
          }
        },
        {
          "template": "full_response_handler"
        }
      ]
    },
    {
      "output_id": "gate_brett_usdt",
      "adapters": [
        {
          "adapter": "fetcher_multiple",
          "config": {
            "custom_node_url_key": "gate_brett_usdt",
            "default_node_host": "https://api.gateio.ws",
            "default_node_path": "/api/v4/spot/tickers?currency_pair=BRETT_USDT",
            "paths": "[\"0.last\"]",
            "timeout": "10"
          }
        },
        {
          "template": "full_response_handler"
        }
      ]
    },
    {
      "adapter": "median_filter",
      "config": {
        "strategy": "nullify",
        "value_ids": "[\"bybit_brett_usdt\", \"kucoin_brett_usdt\", \"htx_brett_usdt\"]",
        "tolerance_percentage": "3"
      }
    },
    {
      "output_id": "brett_usdt",
      "adapter": "weighted_average",
      "config": {
        "value_ids": "[\"bybit_brett_usdt\", \"kucoin_brett_usdt\", \"htx_brett_usdt\"]",
        "weights": "[0.4, 0.4, 0.2]"
      }
    },
    {
      "output_id": "usdt_usd",
      "adapters": [
        {
          "adapter": "oracle_result_fetcher",
          "config": {
            "oracle_id": ".CUSDT",
            "stale_allowance": "900"
          }
        }
      ]
    },
    {
      "output_id": "brett_usd",
      "adapter": "math_filter",
      "config": {
        "operation": "multiply",
        "input_ids": "[\"brett_usdt\",\"usdt_usd\"]"
      }
    },
    {
      "output_id": "brett_usd",
      "input_id": "brett_usd",
      "adapter": "float_handler",
      "config": {
        "operation": "round",
        "precision": "6"
      }
    }
  ]
}