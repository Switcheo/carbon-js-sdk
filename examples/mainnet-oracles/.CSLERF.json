{
  "output_id": "slerf_usd",
  "templates": [
    {
      "template_id": "full_response_handler",
      "adapters": [
        {
          "adapter": "unresponsive_handler",
          "config": {
            "strategy": "use_last",
            "grace_duration": "120"
          }
        },
        {
          "adapter": "unchanged_handler",
          "config": {
            "strategy": "nullify",
            "threshold_duration": "900"
          }
        }
      ]
    }
  ],
  "jobs": [
    {
      "output_id": "bybit_slerf_usdt",
      "adapters": [
        {
          "adapter": "fetcher_multiple",
          "config": {
            "custom_node_url_key": "bybit_perps_slerf_usdt",
            "default_node_host": "https://api-bybit.carbon.network",
            "default_node_path": "/derivatives/v3/public/tickers?category=linear&symbol=SLERFUSDT",
            "paths": "[\"result.list.0.indexPrice\"]",
            "timeout": "10"
          }
        },
        {
          "template": "full_response_handler"
        }
      ]
    },
    {
      "output_id": "htx_slerf_usdt",
      "adapters": [
        {
          "adapter": "fetcher_multiple",
          "config": {
            "custom_node_url_key": "htx_slerf_usdt",
            "default_node_host": "https://api.huobi.pro",
            "default_node_path": "/market/trade?symbol=slerfusdt",
            "paths": "[\"tick.data.0.price\"]",
            "timeout": "10"
          }
        },
        {
          "template": "full_response_handler"
        }
      ]
    },
    {
      "adapter": "median_filter",
      "config": {
        "strategy": "nullify",
        "value_ids": "[\"bybit_slerf_usdt\", \"htx_slerf_usdt\"]",
        "tolerance_percentage": "0.5"
      }
    },
    {
      "output_id": "slerf_usdt",
      "adapter": "weighted_average",
      "config": {
        "value_ids": "[\"bybit_slerf_usdt\", \"htx_slerf_usdt\"]",
        "weights": "[0.5, 0.5]"
      }
    },
    {
      "output_id": "usdt_usd",
      "adapters": [
        {
          "adapter": "oracle_result_fetcher",
          "config": {
            "oracle_id": ".CUSDT",
            "stale_allowance": "900"
          }
        }
      ]
    },
    {
      "output_id": "slerf_usd",
      "adapter": "math_filter",
      "config": {
        "operation": "multiply",
        "input_ids": "[\"slerf_usdt\",\"usdt_usd\"]"
      }
    },
    {
      "output_id": "slerf_usd",
      "input_id": "slerf_usd",
      "adapter": "float_handler",
      "config": {
        "operation": "round",
        "precision": "8"
      }
    }
  ]
}