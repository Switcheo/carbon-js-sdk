{
  "output_id": "bnb_usd",
  "templates": [
    {
      "template_id": "full_response_handler",
      "adapters": [
        {
          "adapter": "unresponsive_handler",
          "config": {
            "strategy": "use_last",
            "grace_duration": "120"
          }
        },
        {
          "adapter": "unchanged_handler",
          "config": {
            "strategy": "nullify",
            "threshold_duration": "900"
          }
        }
      ]
    }
  ],
  "jobs": [
    {
      "output_id": "bybit_bnb_usdt",
      "adapters": [
        {
          "adapter": "fetcher_multiple",
          "config": {
            "custom_node_url_key": "bybit_bnb_usdt",
            "default_node_host": "https://api-bybit.carbon.network",
            "default_node_path": "/v5/market/tickers?category=spot&symbol=BNBUSDT",
            "paths": "[\"result.list.0.lastPrice\"]",
            "timeout": "10"
          }
        },
        {
          "template": "full_response_handler"
        }
      ]
    },
    {
      "output_id": "binance_bnb_usdt",
      "adapters": [
        {
          "adapter": "fetcher_multiple",
          "config": {
            "custom_node_url_key": "binance_bnb_usdt",
            "default_node_host": "https://api-binance-proxy-1.carbon.network",
            "default_node_path": "/api/v3/ticker/price?symbol=BNBUSDT",
            "paths": "[\"price\"]",
            "timeout": "10"
          }
        },
        {
          "template": "full_response_handler"
        }
      ]
    },
    {
      "output_id": "okx_bnb_usdt",
      "adapters": [
        {
          "adapter": "fetcher_multiple",
          "config": {
            "custom_node_url_key": "okx_bnb_usdt",
            "default_node_host": "https://www.okx.com",
            "default_node_path": "/api/v5/market/index-tickers?instId=BNB-USDT",
            "paths": "[\"data.0.idxPx\"]",
            "timeout": "10"
          }
        },
        {
          "template": "full_response_handler"
        }
      ]
    },
    {
      "adapter": "median_filter",
      "config": {
        "strategy": "nullify",
        "value_ids": "[\"bybit_bnb_usdt\", \"binance_bnb_usdt\", \"okx_bnb_usdt\"]",
        "tolerance_percentage": "3"
      }
    },
    {
      "output_id": "bnb_usdt",
      "adapter": "weighted_average",
      "config": {
        "value_ids": "[\"bybit_bnb_usdt\", \"binance_bnb_usdt\", \"okx_bnb_usdt\"]",
        "weights": "[0.22, 0.72, 0.06]"
      }
    },
    {
      "output_id": "usdt_usd",
      "adapters": [
        {
          "adapter": "oracle_result_fetcher",
          "config": {
            "oracle_id": ".CUSDT",
            "stale_allowance": "900"
          }
        }
      ]
    },
    {
      "output_id": "bnb_usd",
      "adapter": "math_filter",
      "config": {
        "operation": "multiply",
        "input_ids": "[\"bnb_usdt\",\"usdt_usd\"]"
      }
    },
    {
      "output_id": "bnb_usd",
      "input_id": "bnb_usd",
      "adapter": "float_handler",
      "config": {
        "operation": "round",
        "precision": "8"
      }
    }
  ]
}