{
  "output_id": "stsomm_usd",
  "templates": [
    {
      "template_id": "rpc_response_handler",
      "adapters": [
        {
          "adapter": "unresponsive_handler",
          "config": {
            "strategy": "use_last",
            "grace_duration": "120"
          }
        }
      ]
    }
  ],
  "jobs": [
    {
      "output_id": "osmo_usd",
      "adapters": [
        {
          "adapter": "oracle_result_fetcher",
          "config": {
            "oracle_id": ".COSMO",
            "stale_allowance": "900"
          }
        }
      ]
    },
    {
      "output_id": "osmosis_somm_osmo_pool",
      "adapters": [
        {
          "adapter": "fetcher_multiple",
          "config": {
            "custom_node_url_key": "osmosis_627_1",
            "default_node_host": "https://api-osmosis.carbon.network",
            "default_node_path": "/osmosis/gamm/v1beta1/pools/627",
            "paths": "[\"pool.pool_assets.1.token.amount\", \"pool.pool_assets.0.token.amount\"]",
            "timeout": "10"
          }
        },
        {
          "template": "rpc_response_handler"
        }
      ]
    },
    {
      "output_id": "osmosis_somm_osmo",
      "adapter": "math_filter",
      "input_id": "osmosis_somm_osmo_pool",
      "config": {
        "operation": "divide"
      }
    },
    {
      "output_id": "somm_usd",
      "adapter": "math_filter",
      "config": {
        "operation": "multiply",
        "input_ids": "[\"osmosis_somm_osmo\", \"osmo_usd\"]"
      }
    },
    {
      "output_id": "stsomm_stride_redemption_rate",
      "adapters": [
        {
          "adapter": "fetcher_multiple",
          "config": {
            "custom_node_url_key": "stride_stsomm_redemption_rate",
            "default_node_host": "https://stride-rest.publicnode.com",
            "default_node_path": "/Stride-Labs/stride/stakeibc/host_zone/sommelier-3",
            "paths": "[\"host_zone.redemption_rate\"]",
            "timeout": "10"
          }
        },
        {
          "template": "rpc_response_handler"
        }
      ]
    },
    {
      "output_id": "stsomm_somm_swap_rate",
      "adapters": [
        {
          "adapter": "fetcher_multiple",
          "config": {
            "custom_node_url_key": "stsomm_somm_swap_rate_1",
            "default_node_host": "https://api-osmosis.carbon.network",
            "default_node_path": "/osmosis/gamm/v1beta1/pools/1120/prices?base_asset_denom=ibc%2F9BBA9A1C257E971E38C1422780CE6F0B0686F0A3085E2D61118D904BFE0F5F5E&quote_asset_denom=ibc%2F5A0060579D24FBE5268BEA74C3281E7FE533D361C41A99307B4998FEC611E46B",
            "paths": "[\"spot_price\"]",
            "timeout": "10"
          }
        },
        {
          "template": "rpc_response_handler"
        }
      ]
    },
    {
      "output_id": "stsomm_redemption_rate",
      "adapter": "math_filter",
      "input_id": "",
      "config": {
        "operation": "min",
        "input_ids": "[\"stsomm_somm_swap_rate\",\"stsomm_stride_redemption_rate\"]"
      }
    },
    {
      "output_id": "stsomm_usd",
      "adapter": "math_filter",
      "config": {
        "operation": "multiply",
        "input_ids": "[\"stsomm_redemption_rate\",\"somm_usd\"]"
      }
    },
    {
      "output_id": "stsomm_usd",
      "input_id": "stsomm_usd",
      "adapter": "float_handler",
      "config": {
        "operation": "round",
        "precision": "6"
      }
    },
    {
      "output_id": "stsomm_usd",
      "input_id": "stsomm_usd",
      "adapter": "unchanged_handler",
      "config": {
        "strategy": "nullify",
        "threshold_duration": "900"
      }
    }
  ]
}
