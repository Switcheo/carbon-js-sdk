{
  "output_id": "rune_usd",
  "templates": [
    {
      "template_id": "full_response_handler",
      "adapters": [
        {
          "adapter": "unresponsive_handler",
          "config": {
            "strategy": "use_last",
            "grace_duration": "120"
          }
        },
        {
          "adapter": "unchanged_handler",
          "config": {
            "strategy": "nullify",
            "threshold_duration": "900"
          }
        }
      ]
    },
    {
      "template_id": "response_handler",
      "adapters": [
        {
          "adapter": "unresponsive_handler",
          "config": {
            "strategy": "use_last",
            "grace_duration": "10"
          }
        }
      ]
    }
  ],
  "jobs": [
    {
      "output_id": "binance_rune_usdt",
      "adapters": [
        {
          "adapter": "fetcher_multiple",
          "config": {
            "custom_node_url_key": "binance_rune_usdt",
            "default_node_host": "https://api-binance-proxy.carbon.network",
            "default_node_path": "/api/v3/ticker/price?symbol=RUNEUSDT",
            "paths": "[\"price\"]",
            "timeout": "10"
          }
        },
        {
          "template": "full_response_handler"
        }
      ]
    },
    {
      "output_id": "usdt_usd",
      "adapters": [
        {
          "adapter": "oracle_result_fetcher",
          "config": {
            "oracle_id": ".CUSDT",
            "stale_allowance": "900"
          }
        }
      ]
    },
    {
      "output_id": "binance_rune_usd",
      "adapter": "math_filter",
      "config": {
        "operation": "multiply",
        "input_ids": "[\"binance_rune_usdt\",\"usdt_usd\"]"
      }
    },
    {
      "output_id": "kucoin_rune_usdt",
      "adapters": [
        {
          "adapter": "fetcher_multiple",
          "config": {
            "custom_node_url_key": "kucoin_rune_usdt",
            "default_node_host": "https://api.kucoin.com",
            "default_node_path": "/api/v1/market/orderbook/level1?symbol=RUNE-USDT",
            "paths": "[\"data.price\"]",
            "timeout": "10"
          }
        },
        {
          "template": "full_response_handler"
        }
      ]
    },
    {
      "output_id": "kucoin_rune_usd",
      "adapter": "math_filter",
      "config": {
        "operation": "multiply",
        "input_ids": "[\"kucoin_rune_usdt\",\"usdt_usd\"]"
      }
    },
    {
      "output_id": "kraken_rune_usd",
      "adapters": [
        {
          "adapter": "fetcher_multiple",
          "config": {
            "custom_node_url_key": "kraken_rune_usd",
            "default_node_host": "https://api.kraken.com",
            "default_node_path": "/0/public/Ticker?pair=RUNEUSD",
            "paths": "[\"result.RUNEUSD.c.0\"]",
            "timeout": "10"
          }
        },
        {
          "template": "full_response_handler"
        }
      ]
    },
    {
      "adapter": "median_filter",
      "config": {
        "strategy": "nullify",
        "value_ids": "[\"binance_rune_usd\", \"kucoin_rune_usd\", \"kraken_rune_usd\"]",
        "tolerance_percentage": "3"
      }
    },
    {
      "output_id": "rune_usd",
      "adapter": "weighted_average",
      "config": {
        "value_ids": "[\"binance_rune_usd\", \"kucoin_rune_usd\", \"kraken_rune_usd\"]",
        "weights": "[0.4, 0.3, 0.3]"
      }
    },
    {
      "output_id": "rune_usd",
      "input_id": "rune_usd",
      "adapter": "float_handler",
      "config": {
        "operation": "round",
        "precision": "8"
      }
    }
  ]
}