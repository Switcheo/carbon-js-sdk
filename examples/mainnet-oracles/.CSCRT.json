{
  "output_id": "scrt_usd",
  "templates": [
    {
      "template_id": "full_response_handler",
      "adapters": [
        {
          "adapter": "unresponsive_handler",
          "config": {
            "strategy": "use_last",
            "grace_duration": "120"
          }
        },
        {
          "adapter": "unchanged_handler",
          "config": {
            "strategy": "nullify",
            "threshold_duration": "900"
          }
        }
      ]
    }
  ],
  "jobs": [
    {
      "output_id": "usdt_usd",
      "adapters": [
        {
          "adapter": "oracle_result_fetcher",
          "config": {
            "oracle_id": ".CUSDT",
            "stale_allowance": "900"
          }
        }
      ]
    },
    {
      "output_id": "bybit_scrt_usdt",
      "adapters": [
        {
          "adapter": "fetcher_multiple",
          "config": {
            "custom_node_url_key": "bybit_scrt_usdt",
            "default_node_host": "https://api-bybit.carbon.network",
            "default_node_path": "/v5/market/tickers?category=spot&symbol=SCRTUSDT",
            "paths": "[\"result.list.0.lastPrice\"]",
            "timeout": "10"
          }
        },
        {
          "template": "full_response_handler"
        }
      ]
    },
    {
      "output_id": "bybit_scrt_usd",
      "adapter": "math_filter",
      "config": {
        "operation": "multiply",
        "input_ids": "[\"bybit_scrt_usdt\",\"usdt_usd\"]"
      }
    },
    {
      "output_id": "binance_scrt_usdt",
      "adapters": [
        {
          "adapter": "fetcher_multiple",
          "config": {
            "custom_node_url_key": "binance_scrt_usdt",
            "default_node_host": "https://api-binance-proxy-1.carbon.network",
            "default_node_path": "/api/v3/ticker/price?symbol=SCRTUSDT",
            "paths": "[\"price\"]",
            "timeout": "10"
          }
        },
        {
          "template": "full_response_handler"
        }
      ]
    },
    {
      "output_id": "binance_scrt_usd",
      "adapter": "math_filter",
      "config": {
        "operation": "multiply",
        "input_ids": "[\"binance_scrt_usdt\",\"usdt_usd\"]"
      }
    },
    {
      "output_id": "kraken_scrt_usd",
      "adapters": [
        {
          "adapter": "fetcher_multiple",
          "config": {
            "custom_node_url_key": "kraken_scrt_usd",
            "default_node_host": "https://api.kraken.com",
            "default_node_path": "/0/public/Ticker?pair=SCRTUSD",
            "paths": "[\"result.SCRTUSD.c.0\"]",
            "timeout": "10"
          }
        },
        {
          "template": "full_response_handler"
        }
      ]
    },
    {
      "adapter": "median_filter",
      "config": {
        "strategy": "nullify",
        "value_ids": "[\"bybit_scrt_usd\", \"binance_scrt_usd\", \"kraken_scrt_usd\"]",
        "tolerance_percentage": "3"
      }
    },
    {
      "output_id": "scrt_usd",
      "adapter": "weighted_average",
      "config": {
        "value_ids": "[\"bybit_scrt_usd\", \"binance_scrt_usd\", \"kraken_scrt_usd\"]",
        "weights": "[0.39, 0.56, 0.05]"
      }
    },
    {
      "output_id": "scrt_usd",
      "input_id": "scrt_usd",
      "adapter": "float_handler",
      "config": {
        "operation": "round",
        "precision": "8"
      }
    }
  ]
}