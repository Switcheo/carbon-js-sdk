{
  "output_id": "sol_usd",
  "templates": [
    {
      "template_id": "response_handler",
      "adapters": [
        {
          "adapter": "unresponsive_handler",
          "config": {
            "strategy": "use_last",
            "grace_duration": "120"
          }
        },
        {
          "adapter": "unchanged_handler",
          "config": {
            "strategy": "nullify",
            "threshold_duration": "900"
          }
        }
      ]
    }
  ],
  "jobs": [
    {
      "output_id": "binance_sol_usdt",
      "adapters": [
        {
          "adapter": "fetcher_multiple",
          "config": {
            "custom_node_url_key": "binance_sol_usdt",
            "default_node_host": "https://api-binance-proxy.carbon.network",
            "default_node_path": "/api/v3/ticker/price?symbol=SOLUSDT",
            "paths": "[\"price\"]",
            "timeout": "10"
          }
        },
        {
          "template": "response_handler"
        }
      ]
    },
    {
      "output_id": "usdt_usd",
      "adapters": [
        {
          "adapter": "oracle_result_fetcher",
          "config": {
            "oracle_id": ".CUSDT",
            "stale_allowance": "900"
          }
        }
      ]
    },
    {
      "output_id": "binance_sol_usd",
      "adapter": "math_filter",
      "config": {
        "operation": "multiply",
        "input_ids": "[\"binance_sol_usdt\",\"usdt_usd\"]"
      }
    },
    {
      "output_id": "kraken_sol_usd",
      "adapters": [
        {
          "adapter": "fetcher_multiple",
          "config": {
            "custom_node_url_key": "kraken_sol_usd",
            "default_node_host": "https://api.kraken.com",
            "default_node_path": "/0/public/Ticker?pair=SOLUSD",
            "paths": "[\"result.SOLUSD.c.0\"]",
            "timeout": "10"
          }
        },
        {
          "template": "response_handler"
        }
      ]
    },
    {
      "output_id": "coinbase_sol_usd",
      "adapters": [
        {
          "adapter": "fetcher_multiple",
          "config": {
            "custom_node_url_key": "coinbase_sol_usd",
            "default_node_host": "https://api.exchange.coinbase.com",
            "default_node_path": "/products/SOL-USD/ticker",
            "paths": "[\"price\"]",
            "timeout": "10"
          }
        },
        {
          "template": "response_handler"
        }
      ]
    },
    {
      "adapter": "median_filter",
      "config": {
        "strategy": "nullify",
        "value_ids": "[\"binance_sol_usd\", \"kraken_sol_usd\",\"coinbase_sol_usd\"]",
        "tolerance_percentage": "3"
      }
    },
    {
      "output_id": "sol_usd",
      "adapter": "weighted_average",
      "config": {
        "value_ids": "[\"binance_sol_usd\",\"kraken_sol_usd\",\"coinbase_sol_usd\"]",
        "weights": "[0.4, 0.2, 0.4]"
      }
    },
    {
      "output_id": "sol_usd",
      "input_id": "sol_usd",
      "adapter": "float_handler",
      "config": {
        "operation": "round",
        "precision": "6"
      }
    }
  ]
}