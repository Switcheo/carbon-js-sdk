{
  "output_id": "stevmos_usd",
  "templates": [
    {
      "template_id": "rpc_response_handler",
      "adapters": [
        {
          "adapter": "unresponsive_handler",
          "config": {
            "strategy": "use_last",
            "grace_duration": "120"
          }
        }
      ]
    }
  ],
  "jobs": [
    {
      "output_id": "osmo_usd",
      "adapters": [
        {
          "adapter": "oracle_result_fetcher",
          "config": {
            "oracle_id": ".COSMO",
            "stale_allowance": "900"
          }
        }
      ]
    },
    {
      "output_id": "osmosis_evmos_osmo_pool",
      "adapters": [
        {
          "adapter": "fetcher_multiple",
          "config": {
            "custom_node_url_key": "osmosis_722_1",
            "default_node_host": "https://api-osmosis.carbon.network",
            "default_node_path": "/osmosis/gamm/v1beta1/pools/722",
            "paths": "[\"pool.pool_assets.1.token.amount\", \"pool.pool_assets.0.token.amount\"]",
            "timeout": "10"
          }
        },
        {
          "template": "rpc_response_handler"
        }
      ]
    },
    {
      "output_id": "osmosis_evmos_osmo",
      "adapter": "math_filter",
      "input_id": "osmosis_evmos_osmo_pool",
      "config": {
        "operation": "divide"
      }
    },
    {
      "output_id": "evmos_usd_12",
      "adapter": "math_filter",
      "config": {
        "operation": "multiply",
        "input_ids": "[\"osmosis_evmos_osmo\", \"osmo_usd\"]"
      }
    },
    {
      "output_id": "evmos_usd",
      "input_id": "evmos_usd_12",
      "adapter": "decimal_handler",
      "config": {
        "operation": "increase",
        "exponent": "12"
      }
    },
    {
      "output_id": "stevmos_stride_redemption_rate",
      "adapters": [
        {
          "adapter": "fetcher_multiple",
          "config": {
            "custom_node_url_key": "stride_stevmos_redemption_rate_1",
            "default_node_host": "https://stride-rest.publicnode.com",
            "default_node_path": "/Stride-Labs/stride/stakeibc/host_zone/evmos_9001-2",
            "paths": "[\"host_zone.redemption_rate\"]",
            "timeout": "10"
          }
        },
        {
          "template": "rpc_response_handler"
        }
      ]
    },
    {
      "output_id": "stevmos_osmosis_redemption_rate",
      "adapters": [
        {
          "adapter": "fetcher_multiple",
          "config": {
            "custom_node_url_key": "osmosis_stevmos_evmos_1",
            "default_node_host": "https://api-osmosis.carbon.network",
            "default_node_path": "/osmosis/gamm/v1beta1/pools/922/prices?base_asset_denom=ibc/6AE98883D4D5D5FF9E50D7130F1305DA2FFA0C652D1DD9C123657C6B4EB2DF8A&quote_asset_denom=ibc/C5579A9595790017C600DD726276D978B9BF314CF82406CE342720A9C7911A01",
            "paths": "[\"spot_price\"]",
            "timeout": "10"
          }
        },
        {
          "template": "rpc_response_handler"
        }
      ]
    },
    {
      "output_id": "stevmos_redemption_rate",
      "adapter": "math_filter",
      "input_id": "",
      "config": {
        "operation": "min",
        "input_ids": "[\"stevmos_osmosis_redemption_rate\",\"stevmos_stride_redemption_rate\"]"
      }
    },
    {
      "output_id": "stevmos_usd",
      "adapter": "math_filter",
      "config": {
        "operation": "multiply",
        "input_ids": "[\"stevmos_redemption_rate\",\"evmos_usd\"]"
      }
    },
    {
      "output_id": "stevmos_usd",
      "input_id": "stevmos_usd",
      "adapter": "float_handler",
      "config": {
        "operation": "round",
        "precision": "6"
      }
    }
  ]
}
