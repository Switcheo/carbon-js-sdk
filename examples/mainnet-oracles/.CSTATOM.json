{
  "output_id": "statom_usd",
  "templates": [
    {
      "template_id": "rpc_response_handler",
      "adapters": [
        {
          "adapter": "unresponsive_handler",
          "config": {
            "strategy": "use_last",
            "grace_duration": "10"
          }
        }
      ]
    }
  ],
  "jobs": [
    {
      "output_id": "atom_usd",
      "adapters": [
        {
          "adapter": "oracle_result_fetcher",
          "config": {
            "oracle_id": ".CATOM",
            "stale_allowance": "900"
          }
        }
      ]
    },
    {
      "output_id": "statom_stride_redemption_rate",
      "adapters": [
        {
          "adapter": "fetcher_multiple",
          "config": {
            "custom_node_url_key": "stride_statom_redemption_rate_1",
            "default_node_host": "https://stride-rest.publicnode.com",
            "default_node_path": "/Stride-Labs/stride/stakeibc/host_zone/cosmoshub-4",
            "paths": "[\"host_zone.redemption_rate\"]",
            "timeout": "10"
          }
        },
        {
          "template": "rpc_response_handler"
        }
      ]
    },
    {
      "output_id": "osmosis_atom_statom_pool",
      "adapters": [
        {
          "adapter": "fetcher_multiple",
          "config": {
            "custom_node_url_key": "osmosis_803_1",
            "default_node_host": "https://api-osmosis.carbon.network",
            "default_node_path": "/osmosis/gamm/v1beta1/pools/803",
            "paths": "[\"pool.pool_assets.0.token.amount\", \"pool.pool_assets.1.token.amount\"]",
            "timeout": "10"
          }
        },
        {
          "template": "rpc_response_handler"
        }
      ]
    },
    {
      "output_id": "statom_osmosis_redemption_rate",
      "adapter": "math_filter",
      "input_id": "osmosis_atom_statom_pool",
      "config": {
        "operation": "divide"
      }
    },
    {
      "output_id": "statom_redemption_rate",
      "adapter": "math_filter",
      "input_id": "",
      "config": {
        "operation": "min",
        "input_ids": "[\"statom_osmosis_redemption_rate\",\"statom_stride_redemption_rate\"]"
      }
    },
    {
      "output_id": "statom_usd",
      "adapter": "math_filter",
      "config": {
        "operation": "multiply",
        "input_ids": "[\"statom_redemption_rate\",\"atom_usd\"]"
      }
    },
    {
      "output_id": "statom_usd",
      "input_id": "statom_usd",
      "adapter": "float_handler",
      "config": {
        "operation": "round",
        "precision": "6"
      }
    },
    {
      "output_id": "statom_usd",
      "input_id": "statom_usd",
      "adapter": "unchanged_handler",
      "config": {
        "strategy": "nullify",
        "threshold_duration": "900"
      }
    }
  ]
}
