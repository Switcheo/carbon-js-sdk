{
  "output_id": "neiroeth_usd",
  "templates": [
    {
      "template_id": "full_response_handler",
      "adapters": [
        {
          "adapter": "unresponsive_handler",
          "config": {
            "strategy": "use_last",
            "grace_duration": "120"
          }
        },
        {
          "adapter": "unchanged_handler",
          "config": {
            "strategy": "nullify",
            "threshold_duration": "900"
          }
        }
      ]
    }
  ],
  "jobs": [
    {
      "output_id": "bybit_neiroeth_usdt",
      "adapters": [
        {
          "adapter": "fetcher_multiple",
          "config": {
            "custom_node_url_key": "bybit_neiroeth_usdt",
            "default_node_host": "https://api-bybit.carbon.network",
            "default_node_path": "/derivatives/v3/public/tickers?category=linear&symbol=NEIROETHUSDT",
            "paths": "[\"result.list.0.indexPrice\"]",
            "timeout": "10"
          }
        },
        {
          "template": "full_response_handler"
        }
      ]
    },
    {
      "output_id": "binance_neiroeth_usdt",
      "adapters": [
        {
          "adapter": "fetcher_multiple",
          "config": {
            "custom_node_url_key": "binance_neiroeth_usdt",
            "default_node_host": "https://fapi-binance-proxy.carbon.network",
            "default_node_path": "/fapi/v2/ticker/price?symbol=NEIROETHUSDT",
            "paths": "[\"price\"]",
            "timeout": "10"
          }
        },
        {
          "template": "full_response_handler"
        }
      ]
    },
    {
      "output_id": "okx_neiroeth_usdt",
      "adapters": [
        {
          "adapter": "fetcher_multiple",
          "config": {
            "custom_node_url_key": "okx_neiroeth_usdt",
            "default_node_host": "https://www.okx.com",
            "default_node_path": "/api/v5/market/index-tickers?instId=NEIROETH-USDT",
            "paths": "[\"data.0.idxPx\"]",
            "timeout": "10"
          }
        },
        {
          "template": "full_response_handler"
        }
      ]
    },
    {
      "adapter": "median_filter",
      "config": {
        "strategy": "nullify",
        "value_ids": "[\"bybit_neiroeth_usdt\", \"binance_neiroeth_usdt\", \"okx_neiroeth_usdt\"]",
        "tolerance_percentage": "3"
      }
    },
    {
      "output_id": "neiroeth_usdt",
      "adapter": "weighted_average",
      "config": {
        "value_ids": "[\"bybit_neiroeth_usdt\", \"binance_neiroeth_usdt\", \"okx_neiroeth_usdt\"]",
        "weights": "[0.10, 0.85, 0.05]"
      }
    },
    {
      "output_id": "usdt_usd",
      "adapters": [
        {
          "adapter": "oracle_result_fetcher",
          "config": {
            "oracle_id": ".CUSDT",
            "stale_allowance": "900"
          }
        }
      ]
    },
    {
      "output_id": "neiroeth_usd",
      "adapter": "math_filter",
      "config": {
        "operation": "multiply",
        "input_ids": "[\"neiroeth_usdt\",\"usdt_usd\"]"
      }
    },
    {
      "output_id": "neiroeth_usd",
      "input_id": "neiroeth_usd",
      "adapter": "float_handler",
      "config": {
        "operation": "round",
        "precision": "8"
      }
    }
  ]
}