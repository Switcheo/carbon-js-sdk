{
  "output_id": "doge_usd",
  "templates": [
    {
      "template_id": "full_response_handler",
      "adapters": [
        {
          "adapter": "unresponsive_handler",
          "config": {
            "strategy": "use_last",
            "grace_duration": "120"
          }
        },
        {
          "adapter": "unchanged_handler",
          "config": {
            "strategy": "nullify",
            "threshold_duration": "900"
          }
        }
      ]
    }
  ],
  "jobs": [
    {
      "output_id": "binance_doge_usdt",
      "adapters": [
        {
          "adapter": "fetcher_multiple",
          "config": {
            "custom_node_url_key": "binance_doge_usdt",
            "default_node_host": "https://api-binance-proxy.carbon.network",
            "default_node_path": "/api/v3/ticker/price?symbol=DOGEUSDT",
            "paths": "[\"price\"]",
            "timeout": "10"
          }
        },
        {
          "template": "full_response_handler"
        }
      ]
    },
    {
      "output_id": "usdt_usd",
      "adapters": [
        {
          "adapter": "oracle_result_fetcher",
          "config": {
            "oracle_id": ".CUSDT",
            "stale_allowance": "900"
          }
        }
      ]
    },
    {
      "output_id": "binance_doge_usd",
      "adapter": "math_filter",
      "config": {
        "operation": "multiply",
        "input_ids": "[\"binance_doge_usdt\",\"usdt_usd\"]"
      }
    },
    {
      "output_id": "bybit_doge_usdt",
      "adapters": [
        {
          "adapter": "fetcher_multiple",
          "config": {
            "custom_node_url_key": "bybit_doge_usdt",
            "default_node_host": "https://api-bybit-proxy.carbon.network",
            "default_node_path": "/spot/v3/public/quote/ticker/price?symbol=DOGEUSDT",
            "paths": "[\"result.price\"]",
            "timeout": "10"
          }
        },
        {
          "template": "full_response_handler"
        }
      ]
    },
    {
      "output_id": "bybit_doge_usd",
      "adapter": "math_filter",
      "config": {
        "operation": "multiply",
        "input_ids": "[\"bybit_doge_usdt\",\"usdt_usd\"]"
      }
    },
    {
      "output_id": "coinbase_doge_usd",
      "adapters": [
        {
          "adapter": "fetcher_multiple",
          "config": {
            "custom_node_url_key": "coinbase_doge_usd",
            "default_node_host": "https://api.exchange.coinbase.com",
            "default_node_path": "/products/DOGE-USD/ticker",
            "paths": "[\"price\"]",
            "timeout": "10"
          }
        },
        {
          "template": "full_response_handler"
        }
      ]
    },
    {
      "adapter": "median_filter",
      "config": {
        "strategy": "nullify",
        "value_ids": "[\"binance_doge_usd\", \"bybit_doge_usd\",\"coinbase_doge_usd\"]",
        "tolerance_percentage": "3"
      }
    },
    {
      "output_id": "doge_usd",
      "adapter": "weighted_average",
      "config": {
        "value_ids": "[\"binance_doge_usd\",\"bybit_doge_usd\",\"coinbase_doge_usd\"]",
        "weights": "[0.45, 0.45, 0.10]"
      }
    },
    {
      "output_id": "doge_usd",
      "input_id": "doge_usd",
      "adapter": "float_handler",
      "config": {
        "operation": "round",
        "precision": "8"
      }
    }
  ]
}