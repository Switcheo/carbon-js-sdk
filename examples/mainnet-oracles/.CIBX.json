{
	"output_id": "ibcx_usd",
	"templates": [
		{
			"template_id": "rpc_response_handler",
			"adapters": [
				{
					"adapter": "unresponsive_handler",
					"config": {
						"strategy": "use_last",
						"grace_duration": "120"
					}
				}
			]
		}
	],
	"jobs": [
		{
			"output_id": "osmo_usd",
			"adapters": [
				{
					"adapter": "oracle_result_fetcher",
					"config": {
						"oracle_id": ".COSMO",
						"stale_allowance": "900"
					}
				}
			]
		},
		{
			"output_id": "ibcx_osmo_6",
			"adapters": [
				{
					"adapter": "fetcher_multiple",
					"config": {
						"custom_node_url_key": "ibcx_osmo_1",
						"default_node_host": "https://api-osmosis.carbon.network",
						"default_node_path": "",
						"paths": "[\"data.swap_result_amount.amount\"]",
						"timeout": "10"
					}
				},
				{
					"template": "rpc_response_handler"
				}
			]
		},
		{
			"output_id": "ibcx_osmo",
			"input_id": "ibcx_osmo_6",
			"adapter": "decimal_handler",
			"config": {
				"operation": "decrease",
				"exponent": "6"
			}
		},
		{
			"output_id": "ibcx_usd",
			"adapter": "math_filter",
			"config": {
				"operation": "multiply",
				"input_ids": "[\"osmo_usd\",\"ibcx_osmo\"]"
			}
		},
		{
			"output_id": "ibcx_usd",
			"input_id": "ibcx_usd",
			"adapter": "float_handler",
			"config": {
				"operation": "round",
				"precision": "8"
			}
		}
	]
}