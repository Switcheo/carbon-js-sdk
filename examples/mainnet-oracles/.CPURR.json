{
  "output_id": "purr_usd",
  "templates": [
    {
      "template_id": "full_response_handler",
      "adapters": [
        {
          "adapter": "unresponsive_handler",
          "config": {
            "strategy": "use_last",
            "grace_duration": "120"
          }
        },
        {
          "adapter": "unchanged_handler",
          "config": {
            "strategy": "nullify",
            "threshold_duration": "900"
          }
        }
      ]
    }
  ],
  "jobs": [
    {
      "output_id": "hyperliquid_purr_usdc",
      "adapters": [
        {
          "adapter": "fetcher_multiple",
          "config": {
            "custom_node_url_key": "hyperliquid_purr_usdc",
            "default_node_host": "https://api.hyperliquid.xyz",
            "default_node_path": "/info",
            "request_body": "{\"type\": \"allMids\"}",
            "paths": "[\"PURR/USDC\"]",
            "timeout": "10"
          }
        },
        {
          "template": "full_response_handler"
        }
      ]
    },
    {
      "output_id": "usdc_usd",
      "adapters": [
        {
          "adapter": "oracle_result_fetcher",
          "config": {
            "oracle_id": ".CUSDC",
            "stale_allowance": "900"
          }
        }
      ]
    },
    {
      "output_id": "purr_usd",
      "adapter": "math_filter",
      "config": {
        "operation": "multiply",
        "input_ids": "[\"hyperliquid_purr_usdc\",\"usdc_usd\"]"
      }
    },
    {
      "output_id": "purr_usd",
      "input_id": "purr_usd",
      "adapter": "float_handler",
      "config": {
        "operation": "round",
        "precision": "8"
      }
