{
  "output_id": "apt_usd",
  "templates": [
    {
      "template_id": "full_response_handler",
      "adapters": [
        {
          "adapter": "unresponsive_handler",
          "config": {
            "strategy": "use_last",
            "grace_duration": "120"
          }
        },
        {
          "adapter": "unchanged_handler",
          "config": {
            "strategy": "nullify",
            "threshold_duration": "900"
          }
        }
      ]
    }
  ],
  "jobs": [
    {
      "output_id": "usdt_usd",
      "adapters": [
        {
          "adapter": "oracle_result_fetcher",
          "config": {
            "oracle_id": ".CUSDT",
            "stale_allowance": "900"
          }
        }
      ]
    },
    {
      "output_id": "bybit_apt_usdt",
      "adapters": [
        {
          "adapter": "fetcher_multiple",
          "config": {
            "custom_node_url_key": "bybit_apt_usdt",
            "default_node_host": "https://api-bybit.carbon.network",
            "default_node_path": "/spot/v3/public/quote/ticker/price?symbol=APTUSDT",
            "paths": "[\"result.price\"]",
            "timeout": "10"
          }
        },
        {
          "template": "full_response_handler"
        }
      ]
    },
    {
      "output_id": "bybit_apt_usd",
      "adapter": "math_filter",
      "config": {
        "operation": "multiply",
        "input_ids": "[\"bybit_apt_usdt\",\"usdt_usd\"]"
      }
    },
    {
      "output_id": "binance_apt_usdt",
      "adapters": [
        {
          "adapter": "fetcher_multiple",
          "config": {
            "custom_node_url_key": "binance_apt_usdt",
            "default_node_host": "https://api-binance-proxy.carbon.network",
            "default_node_path": "/api/v3/ticker/price?symbol=APTUSDT",
            "paths": "[\"price\"]",
            "timeout": "10"
          }
        },
        {
          "template": "full_response_handler"
        }
      ]
    },
    {
      "output_id": "binance_apt_usd",
      "adapter": "math_filter",
      "config": {
        "operation": "multiply",
        "input_ids": "[\"binance_apt_usdt\",\"usdt_usd\"]"
      }
    },
    {
      "output_id": "coinbase_apt_usd",
      "adapters": [
        {
          "adapter": "fetcher_multiple",
          "config": {
            "custom_node_url_key": "coinbase_apt_usd",
            "default_node_host": "https://api.exchange.coinbase.com",
            "default_node_path": "/products/APT-USD/ticker",
            "paths": "[\"price\"]",
            "timeout": "10"
          }
        },
        {
          "template": "full_response_handler"
        }
      ]
    },
    {
      "adapter": "median_filter",
      "config": {
        "strategy": "nullify",
        "value_ids": "[\"bybit_apt_usd\", \"binance_apt_usd\", \"coinbase_apt_usd\"]",
        "tolerance_percentage": "3"
      }
    },
    {
      "output_id": "apt_usd",
      "adapter": "weighted_average",
      "config": {
        "value_ids": "[\"bybit_apt_usd\", \"binance_apt_usd\", \"coinbase_apt_usd\"]",
        "weights": "[0.06, 0.67, 0.27]"
      }
    },
    {
      "output_id": "apt_usd",
      "input_id": "apt_usd",
      "adapter": "float_handler",
      "config": {
        "operation": "round",
        "precision": "8"
      }
    }
  ]
}