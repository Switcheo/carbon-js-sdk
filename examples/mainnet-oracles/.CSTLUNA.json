{
  "output_id": "stluna_usd",
  "templates": [
    {
      "template_id": "response_handler",
      "adapters": [
        {
          "adapter": "unresponsive_handler",
          "config": {
            "strategy": "use_last",
            "grace_duration": "10"
          }
        }
      ]
    }
  ],
  "jobs": [
    {
      "output_id": "luna_usd",
      "adapters": [
        {
          "adapter": "oracle_result_fetcher",
          "config": {
            "oracle_id": ".CLUNA",
            "stale_allowance": "900"
          }
        }
      ]
    },
    {
      "output_id": "stluna_stride_redemption_rate",
      "adapters": [
        {
          "adapter": "fetcher_multiple",
          "config": {
            "custom_node_url_key": "stride_stluna_redemption_rate_1",
            "default_node_host": "https://stride-rest.publicnode.com",
            "default_node_path": "/Stride-Labs/stride/stakeibc/host_zone/phoenix-1",
            "paths": "[\"host_zone.redemption_rate\"]",
            "timeout": "10"
          }
        },
        {
          "template": "response_handler"
        }
      ]
    },
    {
      "output_id": "stluna_astroport_redemption_rate_6",
      "adapters": [
        {
          "adapter": "fetcher_multiple",
          "config": {
            "custom_node_url_key": "astroport_stluna_redemption_rate_1",
            "default_node_host": "https://terra2.tdrsys.com",
            "default_node_path": "",
            "paths": "[\"data.return_amount\"]",
            "timeout": "10"
          }
        },
        {
          "template": "response_handler"
        }
      ]
    },
    {
      "output_id": "stluna_astroport_redemption_rate",
      "input_id": "stluna_astroport_redemption_rate_6",
      "adapter": "decimal_handler",
      "config": {
        "operation": "decrease",
        "exponent": "6"
      }
    },
    {
      "output_id": "stluna_redemption_rate",
      "adapter": "math_filter",
      "input_id": "",
      "config": {
        "operation": "min",
        "input_ids": "[\"stluna_stride_redemption_rate\",\"stluna_astroport_redemption_rate\"]"
      }
    },
    {
      "output_id": "stluna_usd",
      "adapter": "math_filter",
      "config": {
        "operation": "multiply",
        "input_ids": "[\"stluna_redemption_rate\",\"luna_usd\"]"
      }
    },
    {
      "output_id": "stluna_usd",
      "input_id": "stluna_usd",
      "adapter": "float_handler",
      "config": {
        "operation": "round",
        "precision": "8"
      }
    }
  ]
}