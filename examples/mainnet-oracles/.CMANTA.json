{
  "output_id": "manta_usd",
  "templates": [
    {
      "template_id": "full_response_handler",
      "adapters": [
        {
          "adapter": "unresponsive_handler",
          "config": {
            "strategy": "use_last",
            "grace_duration": "120"
          }
        },
        {
          "adapter": "unchanged_handler",
          "config": {
            "strategy": "nullify",
            "threshold_duration": "900"
          }
        }
      ]
    }
  ],
  "jobs": [
    {
      "output_id": "bybit_manta_usdt",
      "adapters": [
        {
          "adapter": "fetcher_multiple",
          "config": {
            "custom_node_url_key": "bybit_manta_usdt",
            "default_node_host": "https://api-bybit-proxy.carbon.network",
            "default_node_path": "/spot/v3/public/quote/ticker/price?symbol=MANTAUSDT",
            "paths": "[\"result.price\"]",
            "timeout": "10"
          }
        },
        {
          "template": "full_response_handler"
        }
      ]
    },
    {
      "output_id": "binance_manta_usdt",
      "adapters": [
        {
          "adapter": "fetcher_multiple",
          "config": {
            "custom_node_url_key": "binance_manta_usdt",
            "default_node_host": "https://api-binance-proxy.carbon.network",
            "default_node_path": "/api/v3/ticker/price?symbol=MANTAUSDT",
            "paths": "[\"price\"]",
            "timeout": "10"
          }
        },
        {
          "template": "full_response_handler"
        }
      ]
    },
    {
      "output_id": "kucoin_manta_usdt",
      "adapters": [
        {
          "adapter": "fetcher_multiple",
          "config": {
            "custom_node_url_key": "kucoin_manta_usdt",
            "default_node_host": "https://api.kucoin.com",
            "default_node_path": "/api/v1/market/orderbook/level1?symbol=MANTA-USDT",
            "paths": "[\"data.price\"]",
            "timeout": "10"
          }
        },
        {
          "template": "full_response_handler"
        }
      ]
    },
    {
      "adapter": "median_filter",
      "config": {
        "strategy": "nullify",
        "value_ids": "[\"bybit_manta_usdt\", \"binance_manta_usdt\", \"kucoin_manta_usdt\"]",
        "tolerance_percentage": "3"
      }
    },
    {
      "output_id": "manta_usdt",
      "adapter": "weighted_average",
      "config": {
        "value_ids": "[\"bybit_manta_usdt\", \"binance_manta_usdt\", \"kucoin_manta_usdt\"]",
        "weights": "[0.4, 0.4, 0.2]"
      }
    },
    {
      "output_id": "usdt_usd",
      "adapters": [
        {
          "adapter": "oracle_result_fetcher",
          "config": {
            "oracle_id": ".CUSDT",
            "stale_allowance": "900"
          }
        }
      ]
    },
    {
      "output_id": "manta_usd",
      "adapter": "math_filter",
      "config": {
        "operation": "multiply",
        "input_ids": "[\"manta_usdt\",\"usdt_usd\"]"
      }
    },
    {
      "output_id": "manta_usd",
      "input_id": "manta_usd",
      "adapter": "float_handler",
      "config": {
        "operation": "round",
        "precision": "8"
      }
    }
  ]
}