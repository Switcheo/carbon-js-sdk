{
  "output_id": "bneo_usd",
  "templates": [
    {
      "template_id": "unchanged_response_handler",
      "adapters": [
        {
          "adapter": "unresponsive_handler",
          "config": {
            "strategy": "use_last",
            "grace_duration": "10"
          }
        },
        {
          "adapter": "unchanged_handler",
          "config": {
            "strategy": "nullify",
            "threshold_duration": "900"
          }
        }
      ]
    },
    {
      "template_id": "response_handler",
      "adapters": [
        {
          "adapter": "unresponsive_handler",
          "config": {
            "strategy": "use_last",
            "grace_duration": "10"
          }
        }
      ]
    }
  ],
  "jobs": [
    {
      "output_id": "usdt_usd",
      "adapters": [
        {
          "adapter": "oracle_result_fetcher",
          "config": {
            "oracle_id": ".CUSDT",
            "stale_allowance": "900"
          }
        }
      ]
    },
    {
      "output_id": "binance_neo_usdt",
      "adapters": [
        {
          "adapter": "fetcher_multiple",
          "config": {
            "custom_node_url_key": "binance_neo_usdt",
            "default_node_host": "https://api-binance-proxy.carbon.network",
            "default_node_path": "/api/v3/ticker/price?symbol=NEOUSDT",
            "paths": "[\"price\"]",
            "timeout": "10"
          }
        },
        {
          "template": "unchanged_response_handler"
        }
      ]
    },
    {
      "output_id": "binance_neo_usd",
      "adapter": "math_filter",
      "config": {
        "operation": "multiply",
        "input_ids": "[\"binance_neo_usdt\",\"usdt_usd\"]"
      }
    },
    {
      "output_id": "okx_neo_usdt",
      "adapters": [
        {
          "adapter": "fetcher_multiple",
          "config": {
            "custom_node_url_key": "okx_neo_usdt",
            "default_node_host": "https://www.okx.com",
            "default_node_path": "/api/v5/market/index-tickers?instId=NEO-USDT",
            "paths": "[\"data.0.idxPx\"]",
            "timeout": "10"
          }
        },
        {
          "template": "unchanged_response_handler"
        }
      ]
    },
    {
      "output_id": "okx_neo_usd",
      "adapter": "math_filter",
      "config": {
        "operation": "multiply",
        "input_ids": "[\"okx_neo_usdt\",\"usdt_usd\"]"
      }
    },
    {
      "output_id": "bitfinex_neo_usd",
      "adapters": [
        {
          "adapter": "fetcher_multiple",
          "config": {
            "custom_node_url_key": "bitfinex_neo_usd",
            "default_node_host": "https://api.bitfinex.com",
            "default_node_path": "/v1/pubticker/neousd",
            "paths": "[\"mid\"]",
            "timeout": "10"
          }
        },
        {
          "template": "unchanged_response_handler"
        }
      ]
    },
    {
      "adapter": "median_filter",
      "config": {
        "strategy": "nullify",
        "value_ids": "[\"binance_neo_usd\", \"okx_neo_usd\",\"bitfinex_neo_usd\"]",
        "tolerance_percentage": "3"
      }
    },
    {
      "output_id": "bneo_usd",
      "adapter": "weighted_average",
      "config": {
        "value_ids": "[\"binance_neo_usd\", \"okx_neo_usd\",\"bitfinex_neo_usd\"]",
        "weights": "[0.6, 0.3, 0.1]"
      }
    },
    {
      "output_id": "bneo_usd",
      "input_id": "bneo_usd",
      "adapter": "float_handler",
      "config": {
        "operation": "round",
        "precision": "6"
      }
    }
  ]
}