{
  "output_id": "mog_usd",
  "templates": [
    {
      "template_id": "full_response_handler",
      "adapters": [
        {
          "adapter": "unresponsive_handler",
          "config": {
            "strategy": "use_last",
            "grace_duration": "120"
          }
        },
        {
          "adapter": "unchanged_handler",
          "config": {
            "strategy": "nullify",
            "threshold_duration": "900"
          }
        }
      ]
    }
  ],
  "jobs": [
    {
      "output_id": "usdt_usd",
      "adapters": [
        {
          "adapter": "oracle_result_fetcher",
          "config": {
            "oracle_id": ".CUSDT",
            "stale_allowance": "900"
          }
        }
      ]
    },
    {
      "output_id": "bybit_mog_usdt",
      "adapters": [
        {
          "adapter": "fetcher_multiple",
          "config": {
            "custom_node_url_key": "bybit_mog_usdt",
            "default_node_host": "https://api-bybit.carbon.network",
            "default_node_path": "/spot/v3/public/quote/ticker/price?symbol=MOGUSDT",
            "paths": "[\"result.price\"]",
            "timeout": "10"
          }
        },
        {
          "template": "full_response_handler"
        }
      ]
    },
    {
      "output_id": "bybit_mog_usd",
      "adapter": "math_filter",
      "config": {
        "operation": "multiply",
        "input_ids": "[\"bybit_mog_usdt\",\"usdt_usd\"]"
      }
    },
    {
      "output_id": "kucoin_mog_usdt",
      "adapters": [
        {
          "adapter": "fetcher_multiple",
          "config": {
            "custom_node_url_key": "kucoin_mog_usdt",
            "default_node_host": "https://api.kucoin.com",
            "default_node_path": "/api/v1/market/orderbook/level1?symbol=MOG-USDT",
            "paths": "[\"data.price\"]",
            "timeout": "10"
          }
        },
        {
          "template": "full_response_handler"
        }
      ]
    },
    {
      "output_id": "kucoin_mog_usd",
      "adapter": "math_filter",
      "config": {
        "operation": "multiply",
        "input_ids": "[\"kucoin_mog_usdt\",\"usdt_usd\"]"
      }
    },
    {
      "output_id": "cryptodotcom_mog_usd",
      "adapters": [
        {
          "adapter": "fetcher_multiple",
          "config": {
            "custom_node_url_key": "cryptodotcom_mog_usd",
            "default_node_host": "https://api.crypto.com",
            "default_node_path": "/exchange/v1/public/get-tickers?instrument_name=MOG_USD",
            "paths": "[\"result.data.0.a\"]",
            "timeout": "10"
          }
        },
        {
          "template": "full_response_handler"
        }
      ]
    },
    {
      "output_id": "bybit_mog_usd",
      "input_id": "bybit_mog_usd",
      "adapter": "decimal_handler",
      "config": {
        "operation": "increase",
        "exponent": "6"
      }
    },
    {
      "output_id": "kucoin_mog_usd",
      "input_id": "kucoin_mog_usd",
      "adapter": "decimal_handler",
      "config": {
        "operation": "increase",
        "exponent": "6"
      }
    },
    {
      "output_id": "cryptodotcom_mog_usd",
      "input_id": "cryptodotcom_mog_usd",
      "adapter": "decimal_handler",
      "config": {
        "operation": "increase",
        "exponent": "6"
      }
    },
    {
      "adapter": "median_filter",
      "config": {
        "strategy": "nullify",
        "value_ids": "[\"bybit_mog_usd\", \"kucoin_mog_usd\", \"cryptodotcom_mog_usd\"]",
        "tolerance_percentage": "3"
      }
    },
    {
      "output_id": "mog_usd",
      "adapter": "weighted_average",
      "config": {
        "value_ids": "[\"bybit_mog_usd\", \"kucoin_mog_usd\", \"cryptodotcom_mog_usd\"]",
        "weights": "[0.29, 0.25, 0.46]"
      }
    },
    {
      "output_id": "mog_usd",
      "input_id": "mog_usd",
      "adapter": "float_handler",
      "config": {
        "operation": "round",
        "precision": "8"
      }
    }
  ]
}